<productinfo
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.mathworks.com/namespace/info/v1/info.xsd">

<?xml-stylesheet type="text/xsl" href="http://www.mathworks.com/namespace/info/v1/info.xsl"?>

<matlabrelease>2009a</matlabrelease>
<name>OpenEarthTools</name>
<type>toolbox</type>
<icon>icons/DeltaresLogo16.PNG</icon>
<help_location>oethelpdocs</help_location>

<list>

<listitem>
<label>Help</label>
<callback>web http://public.deltares.nl/display/OET -helpbrowser</callback>
<icon>$toolbox/matlab/icons/book_mat.gif</icon>
</listitem>

<listitem>
<label>Edit personal config</label>
<callback>edit(fullfile(getenv('APPDATA'), 'OET', 'config'))</callback>
<icon>$toolbox/matlab/icons/pageicon.gif</icon>
</listitem>

<listitem>
<label>Edit Subversion config</label>
<callback>edit(fullfile(getenv('APPDATA'), 'Subversion', 'config'))</callback>
<icon>$toolbox/matlab/icons/pageicon.gif</icon>
</listitem>

<listitem>
<label>Product Page (Web)</label>
<callback>web http://public.deltares.nl/display/OET -browser;</callback>
<icon>$toolbox/matlab/icons/webicon.gif</icon>
</listitem>

<listitem>
<label>Repository</label>
<callback>web https://repos.deltares.nl/repos/OpenEarthTools/trunk/matlab -browser;</callback>
<icon>$toolbox/matlab/icons/webicon.gif</icon>
</listitem>

</list>

</productinfo>
