<?xml version="1.0" encoding="UTF-8"?>
<parameterGroups version="1.0" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/parameters.xsd">

<!-- NRWS + KNMI ############################################################################################# -->

	<parameterGroup id="Waterlevels">
		<parameterType>instantaneous</parameterType>
		<unit>m</unit>
		<valueResolution>0.01</valueResolution>
		<usesDatum>true</usesDatum>
		<parameter id="amplitude" name="Amplitude">
			<shortName>amplitude</shortName>
		</parameter>
		<parameter id="H.meting" name="Waterlevel (measured)">
			<shortName>H.meting</shortName>
		</parameter>
	</parameterGroup>

	<parameterGroup id="Velocities">
		<parameterType>instantaneous</parameterType>
		<unit>m/s</unit>
		<valueResolution>0.01</valueResolution>
		<usesDatum>false</usesDatum>
		<parameter id="velocity_x" name="Velocity-x">
			<shortName>velocity_x</shortName>
		</parameter>
		<parameter id="wind speed" name="wind speed">
			<shortName>wind speed</shortName>
		</parameter>
	</parameterGroup>

	<parameterGroup id="Discharges">
		<parameterType>instantaneous</parameterType>
		<unit>m3/s</unit>
		<valueResolution>0.01</valueResolution>
		<usesDatum>false</usesDatum>
		<parameter id="discharge" name="Discharge">
			<shortName>discharge</shortName>
		</parameter>
	</parameterGroup>

<!-- NOAA NDBC ############################################################################################# -->

<!-- water_temperature:		water_temperature 			[degree_Celsius] "Water Temperature" -->
	<parameterGroup id="Temperature">
		<parameterType>instantaneous</parameterType>
		<unit>oC</unit>
		<valueResolution>0.01</valueResolution>
		<usesDatum>false</usesDatum>
		<parameter id="temperature" name="Temperature">
			<shortName>temperature</shortName>
		</parameter>
	</parameterGroup>
<!-- salinity:			sea_water_salinity 			[] 		"Salinity" -->
	<parameterGroup id="Salinity">
		<parameterType>instantaneous</parameterType>
		<unit>psu</unit>
		<valueResolution>0.1</valueResolution>
		<usesDatum>false</usesDatum>
		<parameter id="salinity" name="Salinity">
			<shortName>salinity</shortName>
		</parameter>
	</parameterGroup>
