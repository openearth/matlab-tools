<filters version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/filters.xsd">
	<defaultFilterId>Import.RWS.waterbase.selection</defaultFilterId>
	<!-- KNMI ###################################################################### -->
	<filter id="Import.KNMI.potwind.all" name="KNMI - potential wind (all)">
		<mapExtentId>North Sea</mapExtentId>
		<timeSeriesSet>
			<moduleInstanceId>Import.KNMI.potwind.all</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>wind speed</parameterId>
			<locationSetId>KNMI.potwind.all</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="nonequidistant"/>
			<relativeViewPeriod unit="day" start="0" end="30"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>5</synchLevel>
		</timeSeriesSet>
	</filter>
	<filter id="Import.KNMI.potwind.selection" name="KNMI - potential wind (selection)">
		<mapExtentId>North Sea</mapExtentId>
