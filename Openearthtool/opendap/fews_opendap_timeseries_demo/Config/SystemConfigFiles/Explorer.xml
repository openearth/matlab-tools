<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2009 sp1 (http://www.altova.com) by ICT (Stichting Deltares) -->
<explorer version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/explorer.xsd">
	<systemInformation>
		<description>WIS database</description>
		<systemCaption>$EXPLORER_SYSTEMCAPTION$</systemCaption>
		<systemHelpFile>$REGION_HOME$\Help.pdf</systemHelpFile>
	</systemInformation>
	<globalDatum>NAP</globalDatum>
	<map>
		<geoDatum>WGS 1984</geoDatum>
		<projection>mercator</projection>
		<defaultExtent id="North Sea">
			<left>-8</left>
			<right>14</right>
			<top>58</top>
			<bottom>50</bottom>
		</defaultExtent>
		<extraExtent id="Netherlands">
			<left>3</left>
			<right>8</right>
			<top>54</top>
			<bottom>50</bottom>
		</extraExtent>
		<extraExtent id="United States">
			<left>-144</left>
			<right>-50</right>
			<top>57</top>
			<bottom>20.75</bottom>
		</extraExtent>
		<scaleBarVisible>true</scaleBarVisible>
		<northArrowVisible>true</northArrowVisible>
		<labelsVisible>true</labelsVisible>
		<!-- WMS: layers with transparency below full ones -->
		<!-- "google maps" look -->
		<wmsLayer id="GDSC (aerial images)">
			<url>http://gdsc.nlr.nl/wms/lufo2005?request=GetCapabilities&amp;service=WMS&amp;</url>
			<wmsLayerName>lufo2005-1m</wmsLayerName>
			<transparent>true</transparent>
			<cacheDir>$REGION_HOME$/localDataStore/gdsc.nlr.nl.gdsc</cacheDir>
		</wmsLayer>
		<openStreetMapLayer id="Open Street Map">
			<url>http://tile.openstreetmap.org</url>
			<cacheDir>$REGION_HOME$/localDataStore/OSM</cacheDir>
		</openStreetMapLayer>
		<wmsLayer id="KNMI geoservices (world raster)">
			<url>http://geoservices.knmi.nl/cgi-bin/worldmaps.cgi?&amp;SERVICE=WMS&amp;</url>
			<wmsLayerName>world_raster</wmsLayerName>
			<transparent>true</transparent>
			<cacheDir>$REGION_HOME$/localDataStore/geoservices.knmi.nl.world_raster</cacheDir>
		</wmsLayer>
		<wmsLayer id="KNMI geoservices (borders)">
			<url>http://geoservices.knmi.nl/cgi-bin/worldmaps.cgi?&amp;SERVICE=WMS&amp;</url>
			<wmsLayerName>nl_world_line</wmsLayerName>
			<transparent>true</transparent>
			<cacheDir>$REGION_HOME$/localDataStore/geoservices.knmi.nl.nl_world_line</cacheDir>
		</wmsLayer>
		<!-- bathymetry -->
		<wmsLayer id="Gebco (bathymetry)">
			<url>http://www.gebco.net/data_and_products/gebco_web_services/web_map_service/mapserv?service=wms&amp;</url>
			<wmsLayerName>GEBCO_08_Grid</wmsLayerName>
			<transparent>true</transparent>
			<cacheDir>$REGION_HOME$/localDataStore/gebco</cacheDir>
		</wmsLayer>
		<wmsLayer id="Emodnet (bathymetry)">
			<url>http://portal.emodnet-hydrography.eu/geoserver/wms?service=WMS&amp;</url>
			<wmsLayerName>emodnet:mean_multicolour</wmsLayerName>
			<transparent>true</transparent>
			<cacheDir>$REGION_HOME$/localDataStore/emodnet</cacheDir>
		</wmsLayer>
		<!-- NMDC demos -->
		<wmsLayer id="NMDC OpenEarth TNO AHN100">
			<url>http://geoservices.knmi.nl/cgi-bin/NMDC_TEST_OPENDAP.cgi?&amp;SERVICE=WMS&amp;VERSION=1.1.1&amp;REQUEST=GetCapabilities</url>
			<wmsLayerName>OpenEarth/opendap/tno/ahn100m/mv100</wmsLayerName>
			<transparent>true</transparent>
			<cacheDir>$REGION_HOME$/localDataStore/nmdc.AHN100</cacheDir>
		</wmsLayer>
		<wmsLayer id="NMDC KNMI RWS DCSMv6 demo">
			<url>http://webgis.nmdc.eu/cgi-bin/rijkswaterstaat.cgi?&amp;SERVICE=WMS&amp;VERSION=1.1.1&amp;REQUEST=GetCapabilities</url>
			<wmsLayerName>test_adaguc/dcsm_v6_hirlam_201112270600/dcsm_v6_hirlam_201112270600_sep</wmsLayerName>
			<transparent>true</transparent>
			<cacheDir>$REGION_HOME$/localDataStore/nmdc.DCSMv6</cacheDir>
		</wmsLayer>
		<!-- KNMI RADNL -->
		<!-- HYMOS-->
		<wmsLayer id="Hymos color @ time=now">
			<url>http://tds.hycom.org/thredds/wms/atl_ops?</url>
			<wmsLayerName>ssh</wmsLayerName>
			<imageFormat>gif</imageFormat>
			<transparent>true</transparent>
			<cacheDir>$REGION_HOME$/localDataStore/hymos.sea_surface_elevation</cacheDir>
		</wmsLayer>
		<wmsLayer id="KNMI geoservices RADNL color @ time=now">
			<url>http://geoservices.knmi.nl/cgi-bin/RADNL_OPER_R___25PCPRR_L3.cgi?&amp;SERVICE=WMS&amp;VERSION=1.1.1&amp;REQUEST=GetCapabilities</url>
			<wmsLayerName>RADNL_OPER_R___25PCPRR_L3_COLOR</wmsLayerName>
			<transparent>true</transparent>
			<cacheDir>$REGION_HOME$/localDataStore/geoservices.knmi.nl.RADNLCOLOR</cacheDir>
		</wmsLayer>
		<wmsLayer id="KNMI geoservices RADNL gray @ time=now">
			<url>http://geoservices.knmi.nl/cgi-bin/RADNL_OPER_R___25PCPRR_L3.cgi?&amp;SERVICE=WMS&amp;VERSION=1.1.1&amp;REQUEST=GetCapabilities</url>
			<wmsLayerName>RADNL_OPER_R___25PCPRR_L3_KNMI</wmsLayerName>
			<transparent>true</transparent>
			<cacheDir>$REGION_HOME$/localDataStore/geoservices.knmi.nl.RADNLGRAY</cacheDir>
		</wmsLayer>
	</map>
	<explorerTasks>
		<explorerTask name="Data Editor">
			<iconFile>%FEWSDIR%/icons/table.gif</iconFile>
			<mnemonic>E</mnemonic>
			<arguments>table</arguments>
			<taskClass>nl.wldelft.fews.gui.plugin.timeseries.TimeSeriesDialog</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<accelerator>ctrl E</accelerator>
		</explorerTask>
		<explorerTask name="Import from OPeNDAP">
			<iconFile>%FEWSDIR%/icons/manforc.png</iconFile>
			<mnemonic>N</mnemonic>
			<!-- ??? ########### -->
			<!-- arguments>ImportPotwind</arguments-->
			<!-- ??? ########### -->
			<taskClass>nl.wldelft.fews.gui.plugin.taskRunDialog.TaskRunDialogFewsExplorerPlugin</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<accelerator>ctrl N</accelerator>
		</explorerTask>
		<explorerTask name="Graph">
			<iconFile>%FEWSDIR%/icons/graph.gif</iconFile>
			<mnemonic>P</mnemonic>
			<arguments>chart</arguments>
			<taskClass>nl.wldelft.fews.gui.plugin.timeseries.PlotsTimeSeriesDialog</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<allowMultipleInstances>true</allowMultipleInstances>
			<doubleClickLocationEnabled>true</doubleClickLocationEnabled>
		</explorerTask>
		<explorerTask name="2D data">
			<iconFile>%FEWSDIR%/iconFiles/grid.gif</iconFile>
			<mnemonic>D</mnemonic>
			<arguments>GridDisplay</arguments>
			<taskClass>nl.wldelft.fews.gui.plugin.grid.GridDisplay</taskClass>
			<toolbarTask>true</toolbarTask>
			<menubarTask>true</menubarTask>
			<allowMultipleInstances>true</allowMultipleInstances>
			<loadAtStartup>true</loadAtStartup>
		</explorerTask>
	</explorerTasks>
	<statusBarConfig>
		<dateTimeFormat>HH:mm:ss</dateTimeFormat>
		<timeGMT>false</timeGMT>
		<timeCurrent>true</timeCurrent>
		<timeLastRefresh>false</timeLastRefresh>
		<statusOfSystem>true</statusOfSystem>
		<mouseCoordinates>true</mouseCoordinates>
		<userName>true</userName>
	</statusBarConfig>
	<dateTime>
		<timeZoneOffset>+01:00</timeZoneOffset>
		<dateTimeFormat>dd-MM-yyyy HH:mm:ss</dateTimeFormat>
		<cardinalTimeStep unit="minute" multiplier="15"/>
	</dateTime>
	<logPanelConfig>
		<clientFilter>
			<clientId>Stand alone</clientId>
			<logFilter>
				<level>INFO</level>
				<eventType>system</eventType>
			</logFilter>
			<logFilter>
				<level>INFO</level>
				<eventType>debug</eventType>
			</logFilter>
		</clientFilter>
		<clientFilter>
			<clientId>Operator Client</clientId>
			<logFilter>
				<level>INFO</level>
				<eventType>system</eventType>
			</logFilter>
			<logFilter>
				<level>INFO</level>
				<eventType>debug</eventType>
			</logFilter>
		</clientFilter>
	</logPanelConfig>
	<panelSizes>
		<loggingPanelSize>0</loggingPanelSize>
		<listsPanelSize>22</listsPanelSize>
		<filterListSize>38</filterListSize>
		<locationListSize>45</locationListSize>
		<parameterListSize>19</parameterListSize>
		<forecastListSize>0</forecastListSize>
	</panelSizes>
	<autoLogin>true</autoLogin>
	<fileMenu>
		<exportTimeSeries visible="true"/>
	</fileMenu>
</explorer>
