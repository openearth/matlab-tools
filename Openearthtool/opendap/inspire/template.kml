<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
<Document>
	<name>document</name>
	<!-- this the style for the upper line -->
	<Style id="polygon">
		<LineStyle>
			<color>ff000000</color>
			<width>3</width>
		</LineStyle>
		<PolyStyle>
			<color>4d66ffff</color>
		</PolyStyle>
	</Style>
	#for $info in $inspireinfo
	<Placemark>
		<name>$info.get('Title')</name>
		<description>
	        <![CDATA[
			 <h1 id="$info.get('Title')">$info.get('Title')</h1>
			 #for $head in $headings
			 <h2>$head</h2>
			 #for $item in $order[$head]
			 <dl>
			 <dt>$item</dt>
			 <dd>$info.get($item,'-')</dd>
			 </dl>
			 #end for
			 #end for
		]]>
		</description>
		<styleUrl>#polygon</styleUrl>
		<Polygon>
		<outerBoundaryIs>
		<LinearRing>
		<coordinates>
		$info.get('Coordinates')
		</coordinates>
		</LinearRing>
		</outerBoundaryIs>
		</Polygon>
		#set $timespans = $info.get('Time spans',[])
		#for $timespan in $timespans
		<TimeSpan id="ID">
		  <begin>$timespan[0]</begin>     <!-- kml:dateTime -->
		  <end>$timespan[1]</end>         <!-- kml:dateTime -->
		</TimeSpan>
		#end for
	</Placemark>
	#end for
</Document>
</kml>
