<?xml version="1.0" encoding="UTF-8"?>
<blackBoxStochModel xmlns="http://www.openda.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.openda.org http://schemas.openda.org/blackBoxStochModelConfig.xsd">
	<modelFactory className="org.openda.model_wflow.WflowModelFactory" workingDirectory=".">
		<arg>River_ModelFactoryConfig.xml</arg>
		<arg>../../River_hbv/run_info.xml</arg>
	</modelFactory>
<!--	<rangeValidationConfigFile>rangeValidationConfig.xml</rangeValidationConfigFile>-->
	<vectorSpecification>
		<state>
			<vector id="LowerZoneStorage"/>
			<vector id="InterceptionStorage"/>
			<vector id="UpperZoneStorage"/>
			<vector id="SurfaceRunoff"/>
			<vector id="FreeWater"/>
			<vector id="SoilMoisture"/>
			<vector id="WaterLevel"/>
			<vector id="DrySnow"/>

<!--

			<vector id="P"/>
			<vector id="TEMP"/>
			<vector id="PET"/>
-->
			<noiseModel id="PrecipNoiseModel" className="org.openda.noiseModels.MapsNoiseModelFactory" workingDirectory=".">
				<configFile>River_PrecipNoise.xml</configFile>
				<exchangeItems>
					<exchangeItem id="precip_noise" operation="add">
						<modelExchangeItem id="P"/>
					</exchangeItem>
				</exchangeItems>
			</noiseModel>
		</state>
		<predictor>
			<subVector id="CIMA1-1182.Q.obs" sourceVectorId="SurfaceRunoff">
				<!-- select row and column indices to get a scalar value 33/31-->
				<!--				<selection index1="32" index2="30"/> -->
				<!--<selection index1="19"/>-->
				<selection index1="2910"/>
			</subVector>
		</predictor>
	</vectorSpecification>
</blackBoxStochModel>
