<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <position>45 10 900 140</position>
    <style>tabpanel</style>
    <tag>coastlinepanel</tag>
    <tabs>
      <tab>
        <tag>coastlinedata</tag>
        <tabstring>Coastline Data</tabstring>
        <elements>
	  <element>
            <tag>pushselectMDA</tag>
            <style>pushselectfile</style>
            <position>10 110 130 20</position>
            <tooltipstring>Select MDA file</tooltipstring>
            <text>Open MDA file</text>
 	    <variable>
              <name>MDAfile</name>
              <type>string</type>
            </variable>
            <extension>*.mda</extension>
            <selectiontext>Select mda File</selectiontext>
            <onchange>ddb_UnibestCL_selectMdaFile</onchange>  
	  </element>
	  <element>
            <tag>pusheditMDA</tag>
            <style>pushbutton</style>
            <position>10 85 130 20</position>
            <text>Edit MDA file</text>	    
            <callback>ddb_UnibestCL_pushEditMdaData</callback>	 	
	  </element>
	  <element>
	    <tag>pushsaveMDA</tag>
	    <style>pushbutton</style>
	    <position>10 60 130 20</position>
	    <text>Save MDA file</text>
	    <callback>ddb_UnibestCL_pushSaveMda</callback>
          </element>
	  <element>
	    <tag>pushsaveMDAas</tag>
	    <style>pushbutton</style>
	    <position>10 35 130 20</position>
	    <text>Save MDA file As ...</text>
	    <callback>ddb_UnibestCL_pushSaveMdaAs</callback>
          </element>          
	</elements>
      </tab>
    </tabs>
  </element>        
</root>
