<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>listdischarges</tag>
    <style>listbox</style>
    <position>45 10 150 140</position>
    <tooltipstring>Select discharge</tooltipstring>
    <text>Observation Points</text>
    <textposition>above-center</textposition>
    <variable>handles.Model(md).Input(ad).activeDischarge</variable>
    <type>integer</type>
    <list>
      <texts>
        <variable>handles.Model(md).Input(ad).dischargeNames</variable>
      </texts>
    </list>
    <onchange>ddb_Delft3DFLOW_discharges</onchange>
    <option1>selectfromlist</option1>
  </element>
  <element>
    <tag>paneldischarges</tag>
    <style>panel</style>
    <position>205 10 170 140</position>
  </element>
  <element>
    <tag>editobsname</tag>
    <style>edit</style>
    <position>245 120 120 20</position>
    <text>Name</text>
    <onchange>ddb_Delft3DFLOW_discharges</onchange>
    <option1>edit</option1>
    <tooltipstring>Edit discharge name</tooltipstring>
    <variable>handles.Model(md).Input(ad).discharges(handles.Model(md).Input(ad).activeDischarge).name</variable>
    <type>string</type>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>pushdeletedischarge</tag>
          <tag>pushadddischarge</tag>
          <tag>pushopendischargesfile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input(ad).nrDischarges</variable>
            <value>0</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
      <dependency>
        <action>update</action>
        <tags>
          <tag>listdischarges</tag>
        </tags>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>editobsm</tag>
    <style>edit</style>
    <position>245 95 50 20</position>
    <text>M</text>
    <onchange>ddb_Delft3DFLOW_discharges</onchange>
    <option1>edit</option1>
    <tooltipstring>Edit discharge index M</tooltipstring>
    <variable>handles.Model(md).Input(ad).discharges(handles.Model(md).Input(ad).activeDischarge).M</variable>
    <type>integer</type>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>pushdeletedischarge</tag>
          <tag>pushadddischarge</tag>
          <tag>pushopendischargesfile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input(ad).nrDischarges</variable>
            <value>0</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
      <dependency>
        <action>update</action>
        <tags>
          <tag>listdischarges</tag>
        </tags>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>editobsn</tag>
    <style>edit</style>
    <position>315 95 50 20</position>
    <text>N</text>
    <onchange>ddb_Delft3DFLOW_discharges</onchange>
    <option1>edit</option1>
    <variable>handles.Model(md).Input(ad).discharges(handles.Model(md).Input(ad).activeDischarge).N</variable>
    <type>integer</type>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>pushdeletedischarge</tag>
          <tag>pushadddischarge</tag>
          <tag>pushopendischargesfile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input(ad).nrDischarges</variable>
            <value>0</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
      <dependency>
        <action>update</action>
        <tags>
          <tag>listdischarges</tag>
        </tags>
      </dependency>
    </dependencies>
    <tooltipstring>Edit discharge index N</tooltipstring>
  </element>
  <element>
    <tag>toggleadddischarge</tag>
    <style>togglebutton</style>
    <position>395 85 70 20</position>
    <text>Add</text>
    <tooltipstring>Add discharge</tooltipstring>
    <onchange>ddb_Delft3DFLOW_discharges</onchange>
    <option1>add</option1>
    <variable>handles.Model(md).Input(ad).addDischarge</variable>
  </element>
  <element>
    <tag>pushdeletedischarge</tag>
    <style>pushbutton</style>
    <position>395 60 70 20</position>
    <text>Delete</text>
    <tooltipstring>Delete discharge</tooltipstring>
    <callback>ddb_Delft3DFLOW_discharges</callback>
    <option1>delete</option1>
  </element>
  <element>
    <tag>toggleselectdischarge</tag>
    <style>togglebutton</style>
    <position>395 35 70 20</position>
    <text>Select</text>
    <tooltipstring>Select discharge</tooltipstring>
    <onchange>ddb_Delft3DFLOW_discharges</onchange>
    <option1>select</option1>
    <variable>handles.Model(md).Input(ad).selectDischarge</variable>
  </element>
  <element>
    <tag>togglechangedischarge</tag>
    <style>togglebutton</style>
    <position>395 10 70 20</position>
    <text>Change</text>
    <tooltipstring>Change discharge</tooltipstring>
    <onchange>ddb_Delft3DFLOW_discharges</onchange>
    <option1>change</option1>
    <variable>handles.Model(md).Input(ad).changeDischarge</variable>
  </element>
  <element>
    <tag>pushopendischarges</tag>
    <style>pushselectfile</style>
    <position>485 60 70 20</position>
    <tooltipstring>Open discharges file</tooltipstring>
    <text>Open</text>
    <variable>handles.Model(md).Input(ad).srcFile</variable>
    <extension>*.src</extension>
    <selectiontext>Select discharges File</selectiontext>
    <onchange>ddb_Delft3DFLOW_discharges</onchange>
    <option1>openfile</option1>    
    <dependencies>
      <dependency>
        <action>update</action>
        <tags>
          <tag>pushsavedischarges</tag>
        </tags>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>pushsavedischarges</tag>
    <style>pushsavefile</style>
    <position>485 35 70 20</position>
    <tooltipstring>Save discharges file</tooltipstring>
    <text>Save</text>
    <variable>handles.Model(md).Input(ad).srcFile</variable>
    <showfilename>no</showfilename>
    <extension>*.src</extension>
    <selectiontext>Select discharges File</selectiontext>
    <onchange>ddb_Delft3DFLOW_discharges</onchange>
    <option1>savefile</option1>
  </element>
</root>
