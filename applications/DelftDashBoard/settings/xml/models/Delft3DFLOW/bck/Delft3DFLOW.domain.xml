<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <position>45 10 900 140</position>
    <style>tabpanel</style>
    <tag>domainpanel</tag>
    <tabs>
      <tab>
        <tag>grid</tag>
        <tabstring>Grid</tabstring>
        <elements>
          <element>
            <tag>pushselectgrid</tag>
            <style>pushselectfile</style>
            <position>10 110 130 20</position>
            <tooltipstring>Select grid file</tooltipstring>
            <text>Open grid</text>
            <variable>GrdFile</variable>
            <vartype>string</vartype>
            <extension>*.grd</extension>
            <selectiontext>Select grid File</selectiontext>
            <onchange>ddb_Delft3DFLOW_selectGridFile</onchange>
          </element>
          <element>
            <tag>pushselectenclosure</tag>
            <style>pushselectfile</style>
            <position>10 85 130 20</position>
            <tooltipstring>Select enclosure file</tooltipstring>
            <text>Open grid enclosure</text>
            <variable>EncFile</variable>
            <vartype>string</vartype>
            <extension>*.enc</extension>
            <selectiontext>Select enclosure File</selectiontext>
            <onchange>ddb_Delft3DFLOW_selectEnclosureFile</onchange>
          </element>
          <element>
            <tag>textgridm</tag>
            <style>text</style>
            <position>10 30</position>
            <prefix>Grid points in M direction : </text>
            <variable>MMax</variable>
            <vartype>integer</vartype>
          </element>
          <element>
            <tag>textgridn</tag>
            <style>text</style>
            <position>10 10</position>
            <prefix>Grid points in N direction : </text>
            <variable>NMax</variable>
            <vartype>integer</vartype>
          </element>
          <element>
            <tag>editlatitude</tag>
            <style>edit</style>
            <position>500 35 50 20</position>
            <variable>Latitude</variable>
            <vartype>real</vartype>
            <text>Latitude (deg)</text>
          </element>
          <element>
            <tag>editorientation</tag>
            <style>edit</style>
            <position>500 10 50 20</position>
            <variable>Orientation</variable>
            <vartype>real</vartype>
            <text>Orientation (deg)</text>
          </element>
          <element>
            <tag>editkmax</tag>
            <style>edit</style>
            <position>500 60 50 20</position>
            <variable>KMax</variable>
            <vartype>integer</vartype>
            <text>Number of layers</text>
            <onchange>ddb_Delft3DFLOW_editKMax</onchange>
          </element>
          <element>
            <tag>pushgeneratelayers</tag>
            <style>pushbutton</style>
            <position>560 60 100 20</position>
            <text>Generate Layers</text>
            <callback>ddb_Delft3DFLOW_pushGenerateLayers</callback>
          </element>
          <element>
            <tag>pushsavelayers</tag>
            <style>pushbutton</style>
            <position>560 35 100 20</position>
            <text>Save Layers</text>
            <callback>ddb_Delft3DFLOW_pushSaveLayers</callback>
          </element>
          <element>
            <tag>pushloadlayers</tag>
            <style>pushbutton</style>
            <position>560 10 100 20</position>
            <text>Load Layers</text>
            <callback>ddb_Delft3DFLOW_pushLoadLayers</callback>
          </element>
          <element>
            <tag>layertable</tag>
            <style>table</style>
            <position>690 25</position>
            <nrrows>5</nrrows>
            <includenumbers>yes</includenumbers>
            <onchange>ddb_Delft3DFLOW_computeSumLayers</onchange>
            <columns>
              <column>
                <style>editreal</style>
                <variable>Thick</variable>
                <width>50</width>
              </column>
            </columns>
          </element>
          <element>
            <tag>sumlayers</tag>
            <style>text</style>
            <position>690 5</position>
            <prefix>Sum : </text>
            <suffix>%</suffix>
            <variable>SumLayers</variable>
            <vartype>real</vartype>
          </element>
          <element>
            <tag>radiosigma</tag>
            <style>radiobutton</style>
            <position>800 85</position>
            <text>Sigma Layers</text>
            <variable>LayerType</variable>
            <vartype>string</vartype>
            <value>Sigma</value>
            <dependencies>
              <dependency>
                <action>on</action>
                <tags>
                  <tag>radioz</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>LayerType</variable>
                    <vartype>string</string>
                    <value>Sigma</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>radioz</tag>
            <style>radiobutton</style>
            <position>800 65</position>
            <text>Z Layers</text>
            <variable>LayerType</variable>
            <vartype>string</vartype>
            <value>Z</value>
            <dependencies>
              <dependency>
                <action>on</action>
                <tags>
                  <tag>radiosigma</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>LayerType</variable>
                    <vartype>string</string>
                    <value>Z</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>editztop</tag>
            <style>edit</style>
            <position>840 35 50 20</position>
            <variable>ZTop</variable>
            <vartype>real</vartype>
            <text>Z top (m)</text>
            <dependencies>
              <dependency>
                <action>enable</action>
                <tags>
                  <tag>editkmax</tag>
                  <tag>radiosigma</tag>
                  <tag>radioz</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>LayerType</variable>
                    <vartype>string</string>
                    <value>Z</value>
                    <operator>eq</operator>
                  </check>
                  <check>
                    <variable>KMax</variable>
                    <vartype>integer</string>
                    <value>1</value>
                    <operator>gt</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>editzbot</tag>
            <style>edit</style>
            <position>840 10 50 20</position>
            <variable>ZBot</variable>
            <vartype>real</vartype>
            <text>Z bottom (m)</text>
            <dependencies>
              <dependency>
                <action>enable</action>
                <tags>
                  <tag>editkmax</tag>
                  <tag>radiosigma</tag>
                  <tag>radioz</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>LayerType</variable>
                    <vartype>string</string>
                    <value>Z</value>
                    <operator>eq</operator>
                  </check>
                  <check>
                    <variable>KMax</variable>
                    <vartype>integer</string>
                    <value>1</value>
                    <operator>gt</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
        </elements>
      </tab>
      <tab>
        <tag>bathymetry</tag>
        <tabstring>Bathymetry</tabstring>
        <elements>
          <element>
            <tag>radiodepfile</tag>
            <style>radiobutton</style>
            <position>20 100</position>
            <text>File</text>
            <variable>depthSource</variable>
            <vartype>string</vartype>
            <value>file</value>
            <dependencies>
              <dependency>
                <action>on</action>
                <tags>
                  <tag>radiodepuniform</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>depthSource</variable>
                    <vartype>string</string>
                    <value>file</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>radiodepuniform</tag>
            <style>radiobutton</style>
            <position>20 75</position>
            <text>Uniform</text>
            <variable>depthSource</variable>
            <vartype>string</vartype>
            <value>uniform</value>
            <dependencies>
              <dependency>
                <action>on</action>
                <tags>
                  <tag>radiodepfile</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>depthSource</variable>
                    <vartype>string</string>
                    <value>uniform</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>pushselectdepfile</tag>
            <style>pushselectfile</style>
            <position>90 100 130 20</position>
            <text>Open Depth File</text>
            <variable>DepFile</variable>
            <vartype>string</vartype>
            <extension>*.dep</extension>
            <selectiontext>Select depth File</selectiontext>
            <onchange>ddb_Delft3DFLOW_selectDepthFile</onchange>
            <tooltipstring>Select Delft3D depth file</tooltipstring>
            <dependencies>
              <dependency>
                <action>enable</action>
                <tags>
                  <tag>radiodepuniform</tag>
                  <tag>radiodepfile</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>depthSource</variable>
                    <vartype>string</string>
                    <value>file</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>edituniformdepth</tag>
            <style>edit</style>
            <position>90 75 50 20</position>
            <text>m below reference level</text>
            <textposition>right</textposition>
            <variable>UniformDepth</variable>
            <vartype>real</vartype>
            <tooltipstring>Enter uniform depth value</tooltipstring>
            <dependencies>
              <dependency>
                <action>enable</action>
                <tags>
                  <tag>radiodepuniform</tag>
                  <tag>radiodepfile</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>depthSource</variable>
                    <vartype>string</vartype>
                    <value>uniform</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
        </elements>
      </tab>
      <tab>
        <tag>drypoints</tag>
        <tabstring>Dry Points</tabstring>
        <elements>
          <element>
            <tag>selectdrypoint</tag>
            <style>listbox</style>
            <position>10 10 150 105</position>
            <tooltipstring>Select dry point</tooltipstring>
            <text>Dry Points</text>
            <textposition>above-center</textposition>
            <list>dryPointNames</list>
            <variable>activeDryPoint</variable>
            <vartype>integer</vartype>
            <onchange>ddb_Delft3DFLOW_selectDryPoints</onchange>
	  </element>
          <element>
            <tag>editdrym1</tag>
            <style>edit</style>
            <position>200 50 50 20</position>
            <text>M1</text>
            <variable>Model(md).Input(ad).DryPoints(s.Model(md).Input(ad).activeDryPoint).M1</variable>
            <subfield3>DryPoints</subfield3>
            <subindex3>
              <variable>activeDryPoint</variable>
              <subfield></subfield>
            </subindex3>
            <vartype>integer</vartype>
            <tooltipstring>Edit dry point index M1</tooltipstring>
          </element>
          <element>
            <tag>editdryn1</tag>
            <style>edit</style>
            <position>280 50 50 20</position>
            <text>N1</text>
            <variable>Model(md).Input(ad).DryPoints(s.Model(md).Input(ad).activeDryPoint).N1</variable>
            <subfield3>DryPoints</subfield3>
            <subindex3>activeDryPoint</subindex3>
            <vartype>integer</vartype>
            <tooltipstring>Edit dry point index M1</tooltipstring>
          </element>
          <element>
            <tag>editdrym2</tag>
            <style>edit</style>
            <position>200 20 50 20</position>
            <text>M2</text>
            <variable>Model(md).Input(ad).DryPoints(s.Model(md).Input(ad).activeDryPoint).M2</variable>
            <subfield3>DryPoints</subfield3>
            <subindex3>activeDryPoint</subindex3>
            <vartype>integer</vartype>
            <tooltipstring>Edit dry point index M1</tooltipstring>
          </element>
          <element>
            <tag>editdryn2</tag>
            <style>edit</style>
            <position>280 20 50 20</position>
            <text>N2</text>
            <variable>Model(md).Input(ad).DryPoints(s.Model(md).Input(ad).activeDryPoint).N2</variable>
            <subfield3>DryPoints</subfield3>
            <subindex3>activeDryPoint</subindex3>
            <vartype>integer</vartype>
            <tooltipstring>Edit dry point index M1</tooltipstring>
          </element>
        </elements>
      </tab>
      <tab>
        <tag>thindams</tag>
        <tabstring>Thin Dams</tabstring>
      </tab>
      <tab>
        <tag>structures</tag>
        <tabstring>Structures</tabstring>
      </tab>
    </tabs>
  </element>
</root>
