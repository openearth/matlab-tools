    <option1>edit</option1>
    <tooltipstring>Edit thin dam index M1</tooltipstring>
    <variable>
      <name>M1</name>
      <type>integer</type>
      <subfield3>
        <variable>
          <name>ThinDams</name>
          <index>
            <variable>
              <name>activeThinDam</name>
              <type>integer</type>
            </variable>
          </index>
        </variable>
      </subfield3>
    </variable>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>pushdeletethindam</tag>
          <tag>pushaddthindam</tag>
          <tag>pushopenthindamsfile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>
              <name>nrThinDams</name>
              <type>integer</type>
            </variable>
            <value>0</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
      <dependency>
        <action>update</action>
        <tags>
          <tag>listthindams</tag>
