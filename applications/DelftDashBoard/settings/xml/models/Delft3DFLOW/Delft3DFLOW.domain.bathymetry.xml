<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>radiodepfile</tag>
    <style>radiobutton</style>
    <position>20 100</position>
    <text>File</text>
    <variable>
      <name>depthSource</name>
      <type>string</type>
    </variable>
    <value>file</value>
    <dependencies>
      <dependency>
        <action>on</action>
        <tags>
          <tag>radiodepuniform</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>
              <name>depthSource</name>
              <type>string</type>
            </variable>
            <value>file</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>radiodepuniform</tag>
    <style>radiobutton</style>
    <position>20 75</position>
    <text>Uniform</text>
    <variable>
      <name>depthSource</name>
      <type>string</type>
    </variable>
    <value>uniform</value>
    <dependencies>
      <dependency>
        <action>on</action>
        <tags>
          <tag>radiodepfile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>
              <name>depthSource</name>
              <type>string</type>
            </variable>
            <value>uniform</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>pushselectdepfile</tag>
    <style>pushselectfile</style>
    <position>90 100 130 20</position>
    <text>Open Depth File</text>
    <variable>
      <name>DepFile</name>
      <type>string</type>
    </variable>
    <extension>*.dep</extension>
    <selectiontext>Select depth File</selectiontext>
    <onchange>ddb_Delft3DFLOW_selectDepthFile</onchange>
    <tooltipstring>Select Delft3D depth file</tooltipstring>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>radiodepuniform</tag>
          <tag>radiodepfile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>
              <name>depthSource</name>
              <type>string</type>
            </variable>
            <value>file</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>edituniformdepth</tag>
    <style>edit</style>
    <position>90 75 50 20</position>
    <text>m below reference level</text>
    <textposition>right</textposition>
    <variable>
      <name>UniformDepth</name>
      <type>real</type>
    </variable>
    <tooltipstring>Enter uniform depth value</tooltipstring>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>radiodepuniform</tag>
          <tag>radiodepfile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>
              <name>depthSource</name>
              <type>string</type>
            </variable>
            <value>uniform</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
</root>
