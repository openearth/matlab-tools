<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <position>45 10 900 140</position>
    <style>tabpanel</style>
    <tag>domainpanel</tag>
    <tabs>
      <tab>
        <tag>grid</tag>
        <tabstring>Grid</tabstring>
        <elements>
          <element>
            <tag>pushselectgrid</tag>
            <style>pushselectfile</style>
            <position>10 110 130 20</position>
            <tooltipstring>Select grid file</tooltipstring>
            <text>Open grid</text>
            <variable>
              <name>GrdFile</name>
              <type>string</type>
            </variable>
            <extension>*.grd</extension>
            <selectiontext>Select grid File</selectiontext>
            <onchange>ddb_Delft3DFLOW_selectGridFile</onchange>
          </element>
          <element>
            <tag>pushselectenclosure</tag>
            <style>pushselectfile</style>
            <position>10 85 130 20</position>
            <tooltipstring>Select enclosure file</tooltipstring>
            <text>Open grid enclosure</text>
            <variable>
              <name>EncFile</name>
              <type>string</type>
            </variable>
            <extension>*.enc</extension>
            <selectiontext>Select enclosure File</selectiontext>
            <onchange>ddb_Delft3DFLOW_selectEnclosureFile</onchange>
          </element>
          <element>
            <tag>textgridm</tag>
            <style>text</style>
            <position>10 30</position>
            <prefix>Grid points in M direction : </text>
            <variable>
              <name>MMax</name>
              <type>integer</type>
            </variable>
          </element>
          <element>
            <tag>textgridn</tag>
            <style>text</style>
            <position>10 10</position>
            <prefix>Grid points in N direction : </text>
            <variable>
              <name>NMax</name>
              <type>integer</type>
            </variable>
          </element>
          <element>
            <tag>editlatitude</tag>
            <style>edit</style>
            <position>500 35 50 20</position>
            <variable>
              <name>Latitude</name>
              <type>real</type>
            </variable>
            <text>Latitude (deg)</text>
          </element>
          <element>
            <tag>editorientation</tag>
            <style>edit</style>
            <position>500 10 50 20</position>
            <variable>
              <name>Orientation</name>
              <type>real</type>
            </variable>
            <text>Orientation (deg)</text>
          </element>
          <element>
            <tag>editkmax</tag>
            <style>edit</style>
            <position>500 60 50 20</position>
            <variable>
              <name>KMax</name>
              <type>integer</type>
            </variable>
            <text>Number of layers</text>
            <onchange>ddb_Delft3DFLOW_editKMax</onchange>
          </element>
          <element>
            <tag>pushgeneratelayers</tag>
            <style>pushbutton</style>
            <position>560 60 100 20</position>
            <text>Generate Layers</text>
            <callback>ddb_Delft3DFLOW_pushGenerateLayers</callback>
          </element>
          <element>
            <tag>pushsavelayers</tag>
            <style>pushbutton</style>
            <position>560 35 100 20</position>
            <text>Save Layers</text>
            <callback>ddb_Delft3DFLOW_pushSaveLayers</callback>
          </element>
          <element>
            <tag>pushloadlayers</tag>
            <style>pushbutton</style>
            <position>560 10 100 20</position>
            <text>Load Layers</text>
            <callback>ddb_Delft3DFLOW_pushLoadLayers</callback>
          </element>
          <element>
            <tag>layertable</tag>
            <style>table</style>
            <position>690 25</position>
            <nrrows>5</nrrows>
            <includenumbers>yes</includenumbers>
            <onchange>ddb_Delft3DFLOW_computeSumLayers</onchange>
            <columns>
              <column>
                <style>editreal</style>
                <variable>
                  <name>Thick</name>
                  <type>real</type>
                </variable>
                <width>50</width>
              </column>
            </columns>
          </element>
          <element>
            <tag>sumlayers</tag>
            <style>text</style>
            <position>690 5</position>
            <prefix>Sum : </text>
            <suffix>%</suffix>
            <variable>
              <name>SumLayers</name>
              <type>real</type>
            </variable>
          </element>
          <element>
            <tag>radiosigma</tag>
            <style>radiobutton</style>
            <position>800 85</position>
            <text>Sigma Layers</text>
            <variable>
              <name>LayerType</name>
              <type>string</type>
            </variable>
            <value>Sigma</value>
            <dependencies>
              <dependency>
                <action>on</action>
                <tags>
                  <tag>radioz</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>
                      <name>LayerType</name>
                      <type>string</type>
                    </variable>
                    <value>Sigma</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>radioz</tag>
            <style>radiobutton</style>
            <position>800 65</position>
            <text>Z Layers</text>
            <variable>
              <name>LayerType</name>
              <type>string</type>
            </variable>
            <value>Z</value>
            <dependencies>
              <dependency>
                <action>on</action>
                <tags>
                  <tag>radiosigma</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>
                      <name>LayerType</name>
                      <type>string</type>
                    </variable>
                    <value>Z</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>editztop</tag>
            <style>edit</style>
            <position>840 35 50 20</position>
            <variable>
              <name>ZTop</name>
              <type>real</type>
            </variable>
            <text>Z top (m)</text>
            <dependencies>
              <dependency>
                <action>enable</action>
                <tags>
                  <tag>editkmax</tag>
                  <tag>radiosigma</tag>
                  <tag>radioz</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>
                      <name>LayerType</name>
                      <type>string</type>
                    </variable>
                    <value>Z</value>
                    <operator>eq</operator>
                  </check>
                  <check>
                    <variable>
                      <name>KMax</name>
                      <type>integer</type>
                    </variable>
                    <value>1</value>
                    <operator>gt</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>editzbot</tag>
            <style>edit</style>
            <position>840 10 50 20</position>
            <variable>
              <name>ZBot</name>
              <type>real</type>
            </variable>
            <text>Z bottom (m)</text>
            <dependencies>
              <dependency>
                <action>enable</action>
                <tags>
                  <tag>editkmax</tag>
                  <tag>radiosigma</tag>
                  <tag>radioz</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>
                      <name>LayerType</name>
                      <type>string</type>
                    </variable>
                    <value>Z</value>
                    <operator>eq</operator>
                  </check>
                  <check>
                    <variable>
                      <name>KMax</name>
                      <type>integer</type>
                    </variable>
                    <value>1</value>
                    <operator>gt</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
        </elements>
      </tab>
      <tab>
        <tag>bathymetry</tag>
        <tabstring>Bathymetry</tabstring>
        <elements>
          <element>
            <tag>radiodepfile</tag>
            <style>radiobutton</style>
            <position>20 100</position>
            <text>File</text>
            <variable>
              <name>depthSource</name>
              <type>string</type>
            </variable>
            <value>file</value>
            <dependencies>
              <dependency>
                <action>on</action>
                <tags>
                  <tag>radiodepuniform</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>
                      <name>depthSource</name>
                      <type>string</type>
                    </variable>
                    <value>file</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>radiodepuniform</tag>
            <style>radiobutton</style>
            <position>20 75</position>
            <text>Uniform</text>
            <variable>
              <name>depthSource</name>
              <type>string</type>
            </variable>
            <value>uniform</value>
            <dependencies>
              <dependency>
                <action>on</action>
                <tags>
                  <tag>radiodepfile</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>
                      <name>depthSource</name>
                      <type>string</type>
                    </variable>
                    <value>uniform</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>pushselectdepfile</tag>
            <style>pushselectfile</style>
            <position>90 100 130 20</position>
            <text>Open Depth File</text>
            <variable>
              <name>DepFile</name>
              <type>string</type>
            </variable>
            <extension>*.dep</extension>
            <selectiontext>Select depth File</selectiontext>
            <onchange>ddb_Delft3DFLOW_selectDepthFile</onchange>
            <tooltipstring>Select Delft3D depth file</tooltipstring>
            <dependencies>
              <dependency>
                <action>enable</action>
                <tags>
                  <tag>radiodepuniform</tag>
                  <tag>radiodepfile</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>
                      <name>depthSource</name>
                      <type>string</type>
                    </variable>
                    <value>file</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
          <element>
            <tag>edituniformdepth</tag>
            <style>edit</style>
            <position>90 75 50 20</position>
            <text>m below reference level</text>
            <textposition>right</textposition>
            <variable>
              <name>UniformDepth</name>
              <type>real</type>
            </variable>
            <tooltipstring>Enter uniform depth value</tooltipstring>
            <dependencies>
              <dependency>
                <action>enable</action>
                <tags>
                  <tag>radiodepuniform</tag>
                  <tag>radiodepfile</tag>
                </tags>
                <checkfor>all</checkfor>
                <checks>
                  <check>
                    <variable>
                      <name>depthSource</name>
                      <type>string</type>
                    </variable>
                    <value>uniform</value>
                    <operator>eq</operator>
                  </check>
                </checks>
              </dependency>
            </dependencies>
          </element>
        </elements>
      </tab>
      <tab>
        <tag>drypoints</tag>
        <tabstring>Dry Points</tabstring>
        <elements>
          <element>
            <tag>selectdrypoint</tag>
            <style>listbox</style>
            <position>10 10 150 105</position>
            <tooltipstring>Select dry point</tooltipstring>
            <text>Dry Points</text>
            <textposition>above-center</textposition>
            <list>
              <variable>
                <name>dryPointNames</name>
                <type>string</type>
              </variable>
            </list>
            <variable>
              <name>activeDryPoint</name>
              <type>integer</type>
            </variable>
            <onchange>ddb_Delft3DFLOW_selectDryPoints</onchange>
	  </element>
          <element>
            <tag>editdrym1</tag>
            <style>edit</style>
            <position>200 50 50 20</position>
            <text>M1</text>
            <variable>
              <name>M1</name>
              <type>integer</type>
              <subfield3>
                <variable>
                  <name>DryPoints</name>
                  <index>
                    <variable>
                      <name>activeDryPoint</name>
                      <type>integer</type>
                    </variable>
                  </index>
                </variable>
              </subfield3>
            </variable>
            <tooltipstring>Edit dry point index M1</tooltipstring>
          </element>
          <element>
            <tag>editdryn1</tag>
            <style>edit</style>
            <position>280 50 50 20</position>
            <text>N1</text>
            <variable>
              <name>N1</name>
              <type>integer</type>
              <subfield3>
                <variable>
                  <name>DryPoints</name>
                  <index>
                    <variable>
                      <name>activeDryPoint</name>
                      <type>integer</type>
                    </variable>
                  </index>
                </variable>
              </subfield3>
            </variable>
            <tooltipstring>Edit dry point index N1</tooltipstring>
          </element>
          <element>
            <tag>editdrym2</tag>
            <style>edit</style>
            <position>200 20 50 20</position>
            <text>M2</text>
            <variable>
              <name>M2</name>
              <type>integer</type>
              <subfield3>
                <variable>
                  <name>DryPoints</name>
                  <index>
                    <variable>
                      <name>activeDryPoint</name>
                      <type>integer</type>
                    </variable>
                  </index>
                </variable>
              </subfield3>
            </variable>
            <tooltipstring>Edit dry point index M2</tooltipstring>
          </element>
          <element>
            <tag>editdryn2</tag>
            <style>edit</style>
            <position>280 20 50 20</position>
            <text>N2</text>
            <variable>
              <name>N2</name>
              <type>integer</type>
              <subfield3>
                <variable>
                  <name>DryPoints</name>
                  <index>
                    <variable>
                      <name>activeDryPoint</name>
                      <type>integer</type>
                    </variable>
                  </index>
                </variable>
              </subfield3>
            </variable>
            <tooltipstring>Edit dry point index N2</tooltipstring>
          </element>
        </elements>
      </tab>
      <tab>
        <tag>thindams</tag>
        <tabstring>Thin Dams</tabstring>
      </tab>
      <tab>
        <tag>structures</tag>
        <tabstring>Structures</tabstring>
      </tab>
    </tabs>
  </element>
</root>
