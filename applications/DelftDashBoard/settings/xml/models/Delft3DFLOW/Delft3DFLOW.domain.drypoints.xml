<root>
  <element>
    <tag>listdrypoints</tag>
    <style>listbox</style>
    <position>10 10 150 105</position>
    <tooltipstring>Select dry point</tooltipstring>
    <text>Dry Points</text>
    <textposition>above-center</textposition>
    <variable>
      <name>activeDryPoint</name>
      <type>integer</type>
    </variable>
    <list>
      <variable>
        <name>dryPointNames</name>
      </variable>
    </list>
    <onchange>ddb_Delft3DFLOW_dryPoints</onchange>
    <option1>selectfromlist</option1>
  </element>
  <element>
    <tag>paneldrypoints</tag>
    <style>panel</style>
    <position>170 10 170 70</position>
  </element>
  <element>
    <tag>editdrym1</tag>
    <style>edit</style>
    <position>200 50 50 20</position>
    <text>M1</text>
    <onchange>ddb_Delft3DFLOW_dryPoints</onchange>
    <option1>edit</option1>
    <tooltipstring>Edit dry point index M1</tooltipstring>
    <variable>
      <name>M1</name>
      <type>integer</type>
      <subfield3>
        <variable>
          <name>DryPoints</name>
          <index>
            <variable>
              <name>activeDryPoint</name>
              <type>integer</type>
            </variable>
          </index>
        </variable>
      </subfield3>
    </variable>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>pushdeletedrypoint</tag>
          <tag>pushadddrypoint</tag>
          <tag>pushopendrypointsfile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>
              <name>nrDryPoints</name>
              <type>integer</type>
            </variable>
            <value>0</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
      <dependency>
        <action>update</action>
        <tags>
          <tag>listdrypoints</tag>
        </tags>
      </dependency>
    </dependencies>
