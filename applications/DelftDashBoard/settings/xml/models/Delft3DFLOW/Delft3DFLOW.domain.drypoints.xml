    <position>280 20 50 20</position>
    <text>N2</text>
    <onchange>ddb_Delft3DFLOW_dryPoints</onchange>
    <option1>edit</option1>
    <variable>handles.Model(md).Input(ad).dryPoints(handles.Model(md).Input(ad).activeDryPoint).N2</variable>
    <type>integer</type>
    <dependencies>
