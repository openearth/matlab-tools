    <type>integer</type>
    <text>Water Level</text>
    <variable>handles.Toolbox(tb).Input.options.waterLevel.BC.source</variable>
    <tooltipstring>Select an source for water level boundary conditions</tooltipstring> 
    <list>
      <texts>
        <text>Astronomic</text>
        <text>File</text>
        <text>Astronomic + File</text>
        <text>Constant</text>
      </texts>
    </list>
  </element> 

  <element>
    <tag>checkautowl</tag>
    <style>checkbox</style>
    <position>190 105</position>
    <text>Automatic</text>
    <variable>handles.Toolbox(tb).Input.autoWL</variable>
    <tooltipstring>Automatically generate water levels from tide database</tooltipstring> 
    <dependencies>
      <dependency>
        <action>visible</action>
        <tags>
          <tag>selectwlsource</tag>
        </tags>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.waterLevel.BC.source</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
          <check>
            <variable>handles.Toolbox(tb).Input.options.waterLevel.BC.source</variable>
            <value>3</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element>  

