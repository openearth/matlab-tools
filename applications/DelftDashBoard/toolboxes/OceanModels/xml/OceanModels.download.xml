    <textposition>left</textposition>
    <tooltipstring>Edit file name for output</tooltipstring> 
  </element>
  <element>
    <tag>editfolder</tag>
    <style>edit</style>
    <position>50 50 100 20</position>
    <variable>handles.Toolbox(tb).Input.folder</variable>
