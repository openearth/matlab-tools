        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.salinity.BC.source</variable>
            <value>2</value>
            <operator>eq</operator>
          </check>
          <check>
            <variable>handles.Toolbox(tb).Input.options.temperature.BC.source</variable>
