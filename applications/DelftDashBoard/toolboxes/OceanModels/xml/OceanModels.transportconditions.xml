          <tag>selecttemperaturesource</tag>
        </tags>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.temperature.BC.source</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element> 
  <element>
    <tag>selecttemperatureprofile</tag>
