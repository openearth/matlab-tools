<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>selectsalinitysource</tag>
    <position>110 105 100 20</position>
    <style>popupmenu</style>
    <type>integer</type>
    <text>Salinity (ppt)</text>
    <variable>handles.Toolbox(tb).Input.options.salinity.BC.source</variable>
    <tooltipstring>Select source for salinity boundary conditions</tooltipstring> 
    <list>
      <texts>
        <text>Constant</text>
        <text>File</text>
        <text>Profile</text>
      </texts>
      <values>
        <value>4</value>
        <value>2</value>
        <value>5</value>
      </values> 
    </list>
  </element>
  <element>
    <tag>editsalinity</tag>
    <position>330 105 50 20</position>
    <style>edit</style>
    <type>real</type>
    <text>Salinity (ppt)</text>
    <variable>handles.Toolbox(tb).Input.options.salinity.BC.constant</variable>
    <tooltipstring>Edit constant salinity (ppt)</tooltipstring> 
    <dependencies>
      <dependency>
        <action>visible</action>
        <tags>
          <tag>selectsalinitysource</tag>
        </tags>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.salinity.BC.source</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element> 
  <element>
    <tag>selectsalinityprofile</tag>
    <style>pushselectfile</style>
    <position>230 105 100 20</position>
    <tooltipstring>Select salinity profile file</tooltipstring>
    <text>Profile File</text>
    <variable>handles.Toolbox(tb).Input.options.salinity.BC.profileFile</variable>
    <extension>*.prf</extension>
    <selectiontext>Select *.prf file</selectiontext>
    <dependencies>
      <dependency>
        <action>visible</action>
        <tags>
          <tag>selectsalinitysource</tag>
        </tags>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.salinity.BC.source</variable>
            <value>3</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element> 


  <element>
