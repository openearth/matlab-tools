    <position>850 40 50 20</position>
    <style>edit</style>
    <type>real</type>
    <text>Time Step (min)</text>
    <variable>handles.Toolbox(tb).Input.options.bccTimeStep</variable>
    <tooltipstring>Edit time step in bcc file (min)</tooltipstring> 
  </element> 

  <element>
    <tag>generatebccfile</tag>
    <style>pushbutton</style>
    <position>850 10 100 20</position>
    <text>Generate BCC</text>
    <callback>ddb_OceanModelsToolbox_nestBcc</callback>
