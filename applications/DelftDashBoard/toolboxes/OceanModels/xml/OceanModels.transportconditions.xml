    <list>
      <texts>
        <text>Constant</text>
        <text>File</text>
        <text>Profile</text>
      </texts>
      <values>
        <value>4</value>
        <value>2</value>
        <value>5</value>
      </values> 
    </list>
  </element>
  <element>
    <tag>edittemperature</tag>
    <position>330 80 50 20</position>
    <style>edit</style>
    <type>real</type>
    <text>Temperature (degC)</text>
