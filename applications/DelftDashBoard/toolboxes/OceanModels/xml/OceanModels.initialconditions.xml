<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>selectwlsource</tag>
    <position>110 110 100 20</position>
    <style>popupmenu</style>
    <type>integer</type>
    <text>Water Level</text>
    <variable>handles.Toolbox(tb).Input.options.waterLevel.IC.source</variable>
    <tooltipstring>Select an source for water level boundary conditions</tooltipstring> 
    <list>
      <texts>
        <text>Constant</text>
      </texts>
      <values>
        <value>4</value>
      </values> 
    </list>
  </element> 
  <element>
    <tag>editzcor</tag>
    <position>330 110 50 20</position>
    <style>edit</style>
    <type>real</type>
    <text>Correction (m)</text>
    <variable>handles.Toolbox(tb).Input.options.waterLevel.IC.constant</variable>
    <tooltipstring>Edit water level correction (m)</tooltipstring> 
  </element> 
  <element>
    <tag>selectcurrentsource</tag>
    <position>110 85 100 20</position>
    <style>popupmenu</style>
    <type>integer</type>
    <text>Currents</text>
    <variable>handles.Toolbox(tb).Input.options.current.IC.source</variable>
    <tooltipstring>Select an source for water level boundary conditions</tooltipstring> 
    <list>
      <texts>
        <text>Constant</text>
        <text>File</text>
      </texts>
      <values>
        <value>4</value>
        <value>2</value>
      </values> 
    </list>
  </element> 

  <element>
    <tag>selectsalinitysource</tag>
    <position>110 60 100 20</position>
    <style>popupmenu</style>
    <type>integer</type>
    <text>Salinity (ppt)</text>
    <variable>handles.Toolbox(tb).Input.options.salinity.IC.source</variable>
    <tooltipstring>Select source for salinity boundary conditions</tooltipstring> 
    <list>
      <texts>
        <text>Constant</text>
        <text>File</text>
        <text>Profile</text>
      </texts>
      <values>
        <value>4</value>
        <value>2</value>
        <value>5</value>
      </values> 
    </list>
  </element>
  <element>
    <tag>editsalinity</tag>
    <position>330 60 50 20</position>
    <style>edit</style>
    <type>real</type>
    <text>Salinity (ppt)</text>
    <variable>handles.Toolbox(tb).Input.options.salinity.IC.constant</variable>
    <tooltipstring>Edit constant salinity (ppt)</tooltipstring> 
    <dependencies>
      <dependency>
        <action>visible</action>
        <tags>
          <tag>selectsalinitysource</tag>
        </tags>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.salinity.IC.source</variable>
            <value>4</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element> 
  <element>
    <tag>selectsalinityprofile</tag>
    <style>pushselectfile</style>
    <position>230 60 100 20</position>
    <tooltipstring>Select salinity profile file</tooltipstring>
    <text>Profile File</text>
    <variable>handles.Toolbox(tb).Input.options.salinity.IC.profileFile</variable>
    <extension>*.prf</extension>
    <selectiontext>Select *.prf file</selectiontext>
    <dependencies>
      <dependency>
        <action>visible</action>
        <tags>
          <tag>selectsalinitysource</tag>
        </tags>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.salinity.IC.source</variable>
            <value>5</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element> 


  <element>
    <tag>selecttemperaturesource</tag>
    <position>110 35 100 20</position>
    <style>popupmenu</style>
    <type>integer</type>
    <text>Temperature (degC)</text>
    <variable>handles.Toolbox(tb).Input.options.temperature.IC.source</variable>
    <tooltipstring>Select source for temperature boundary conditions</tooltipstring> 
    <list>
      <texts>
        <text>Constant</text>
        <text>File</text>
        <text>Profile</text>
      </texts>
      <values>
        <value>4</value>
        <value>2</value>
        <value>5</value>
      </values> 
    </list>
  </element>
  <element>
    <tag>edittemperature</tag>
    <position>330 35 50 20</position>
    <style>edit</style>
    <type>real</type>
    <text>Temperature (degC)</text>
    <variable>handles.Toolbox(tb).Input.options.temperature.IC.constant</variable>
    <tooltipstring>Edit constant temperature (degC)</tooltipstring> 
    <dependencies>
      <dependency>
        <action>visible</action>
        <tags>
          <tag>selecttemperaturesource</tag>
        </tags>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.temperature.IC.source</variable>
            <value>4</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element> 
  <element>
    <tag>selecttemperatureprofile</tag>
    <style>pushselectfile</style>
    <position>230 35 100 20</position>
    <tooltipstring>Select temperature profile file</tooltipstring>
    <text>Profile File</text>
    <variable>handles.Toolbox(tb).Input.options.temperature.IC.profileFile</variable>
    <extension>*.prf</extension>
    <selectiontext>Select *.prf file</selectiontext>
    <dependencies>
      <dependency>
        <action>visible</action>
        <tags>
          <tag>selecttemperaturesource</tag>
        </tags>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.temperature.IC.source</variable>
            <value>5</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element> 


  <element>
    <tag>editname</tag>
    <style>edit</style>
    <position>110 10 100 20</position>
    <variable>handles.Toolbox(tb).Input.name</variable>
    <type>string</type>
    <text>Data File</text>
    <textposition>left</textposition>
    <tooltipstring>Edit file name for data files</tooltipstring> 
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>selectsalinitysource</tag>
          <tag>selecttemperaturesource</tag>
          <tag>selectcurrentsource</tag>
        </tags>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.current.IC.source</variable>
            <value>2</value>
            <operator>eq</operator>
          </check>
          <check>
            <variable>handles.Toolbox(tb).Input.options.salinity.IC.source</variable>
            <value>3</value>
            <operator>eq</operator>
          </check>
          <check>
            <variable>handles.Toolbox(tb).Input.options.temperature.IC.source</variable>
            <value>2</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element>
  <element>
    <tag>editfolder</tag>
    <style>edit</style>
    <position>330 10 100 20</position>
    <variable>handles.Toolbox(tb).Input.folder</variable>
    <type>string</type>
    <text>Data Folder</text>
    <textposition>left</textposition>
    <tooltipstring>Edit folder name for data files</tooltipstring> 
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>selectsalinitysource</tag>
          <tag>selecttemperaturesource</tag>
          <tag>selectcurrentsource</tag>
        </tags>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.options.current.IC.source</variable>
            <value>2</value>
            <operator>eq</operator>
          </check>
          <check>
            <variable>handles.Toolbox(tb).Input.options.salinity.IC.source</variable>
            <value>3</value>
            <operator>eq</operator>
          </check>
          <check>
            <variable>handles.Toolbox(tb).Input.options.temperature.IC.source</variable>
            <value>2</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element>
  <element>
    <tag>generateinifile</tag>
    <style>pushbutton</style>
    <position>850 10 100 20</position>
    <text>Generate INI</text>
    <callback>ddb_OceanModelsToolbox_nestIni</callback>
    <option1>generateini</option1>
    <tooltipstring>Generate *.ini file</tooltipstring>
  </element> 
</root>
