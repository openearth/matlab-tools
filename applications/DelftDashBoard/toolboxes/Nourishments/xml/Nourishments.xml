<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <toolbox>Nourishments</toolbox>
  <longname>Nourishments</longname>
  <enable>1</enable>
  <elements>
    <element>
      <tag>nourishmentspanel</tag>
      <style>panel</style>
      <position>40 10 900 160</position>
      <text>Nourishments</text>
    </element> 
    <element>
      <tag>editxmin</tag>
      <style>edit</style>
      <position>90 130 50 20</position>
      <text>X Min</text>
      <textposition>left</textposition>
      <onchange>ddb_NourishmentsToolbox</onchange>
      <option1>editoutline</option1>
      <variable>handles.Toolbox(tb).Input.xLim(1)</variable>
      <type>real</type>
      <tooltipstring>Edit minimum x coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editxmax</tag>
      <style>edit</style>
      <position>90 105 50 20</position>
      <text>X Max</text>
      <textposition>left</textposition>
      <onchange>ddb_NourishmentsToolbox</onchange>
      <option1>editoutline</option1>
      <variable>handles.Toolbox(tb).Input.xLim(2)</variable>
      <type>real</type>
      <tooltipstring>Edit maximum x coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editymin</tag>
      <style>edit</style>
      <position>90 80 50 20</position>
      <text>Y Min</text>
      <textposition>left</textposition>
      <onchange>ddb_NourishmentsToolbox</onchange>
      <option1>editoutline</option1>
      <variable>handles.Toolbox(tb).Input.yLim(1)</variable>
      <type>real</type>
      <tooltipstring>Edit minimum y coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editymax</tag>
      <style>edit</style>
      <position>90 55 50 20</position>
      <text>Y Max</text>
      <textposition>left</textposition>
      <onchange>ddb_NourishmentsToolbox</onchange>
      <option1>editoutline</option1>
      <variable>handles.Toolbox(tb).Input.yLim(2)</variable>
      <type>real</type>
      <tooltipstring>Edit maximum y coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editdx</tag>
      <style>edit</style>
      <position>220 130 70 20</position>
      <text>Grid Size</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.dX</variable>
      <type>real</type>
      <tooltipstring>Edit grid size</tooltipstring>
    </element> 
    <element>
      <tag>editvolume</tag>
      <style>edit</style>
      <position>220 80 70 20</position>
      <text>Volume</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.nourishmentVolume</variable>
      <type>real</type>
      <tooltipstring>Edit volume</tooltipstring>
    </element> 
    <element>
      <tag>drawrectangle</tag>
      <style>pushbutton</style>
      <position>300 130 100 20</position>
      <text>Draw Outline</text>
      <callback>ddb_NourishmentsToolbox</callback>
      <option1>drawrectangle</option1>
      <tooltipstring>Draw rectangle</tooltipstring>
    </element> 
    <element>
      <tag>drawpolygon</tag>
      <style>pushbutton</style>
      <position>300 105 80 20</position>
      <text>Draw Polygon</text>
      <callback>ddb_NourishmentsToolbox</callback>
      <option1>drawpolygon</option1>
    </element> 
  <element>
    <tag>loadcurrents</tag>
    <style>pushselectfile</style>
    <position>300 80 80 20</position>
    <text>Load Currents</text>
    <variable>handles.Toolbox(tb).Input.currentsFile</variable>
    <onchange>ddb_NourishmentsToolbox</onchange>
    <option1>loadcurrents</option1>
    <showfilename>no</showfilename> 
    <extension>*.mat</extension>
    <selectiontext>Select currents file</selectiontext>
    <tooltipstring>Select currents file</tooltipstring> 
  </element>
    <element>
      <tag>computenourishment</tag>
      <style>pushbutton</style>
      <position>500 50 100 20</position>
      <text>Compute</text>
      <callback>ddb_NourishmentsToolbox</callback>
      <option1>computenourishment</option1>
      <tooltipstring>Compute</tooltipstring>
    </element> 
  </elements> 
</root>
