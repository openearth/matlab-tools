<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <toolbox>Nourishments</toolbox>
  <longname>Nourishments</longname>
  <enable>1</enable>
  <elements>
    <element>
      <tag>nourishmentspanel</tag>
      <style>panel</style>
      <position>40 10 900 165</position>
      <text>Nourishments</text>
    </element> 
    <element>
      <tag>editxmin</tag>
      <style>edit</style>
      <position>90 135 60 20</position>
      <text>X Min</text>
      <textposition>left</textposition>
      <onchange>ddb_NourishmentsToolbox</onchange>
      <option1>editoutline</option1>
      <variable>handles.Toolbox(tb).Input.xLim(1)</variable>
      <type>real</type>
      <tooltipstring>Edit minimum x coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editxmax</tag>
      <style>edit</style>
      <position>190 135 60 20</position>
      <text>X Max</text>
      <textposition>left</textposition>
      <onchange>ddb_NourishmentsToolbox</onchange>
      <option1>editoutline</option1>
      <variable>handles.Toolbox(tb).Input.xLim(2)</variable>
      <type>real</type>
      <tooltipstring>Edit maximum x coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editymin</tag>
      <style>edit</style>
      <position>90 110 60 20</position>
      <text>Y Min</text>
      <textposition>left</textposition>
      <onchange>ddb_NourishmentsToolbox</onchange>
      <option1>editoutline</option1>
      <variable>handles.Toolbox(tb).Input.yLim(1)</variable>
      <type>real</type>
      <tooltipstring>Edit minimum y coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editymax</tag>
      <style>edit</style>
      <position>190 110 60 20</position>
      <text>Y Max</text>
      <textposition>left</textposition>
      <onchange>ddb_NourishmentsToolbox</onchange>
      <option1>editoutline</option1>
      <variable>handles.Toolbox(tb).Input.yLim(2)</variable>
      <type>real</type>
      <tooltipstring>Edit maximum y coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editdx</tag>
      <style>edit</style>
      <position>190 85 60 20</position>
      <text>Spacing (m)</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.dX</variable>
      <type>real</type>
      <tooltipstring>Edit grid spacing (in metres)</tooltipstring>
    </element> 
    <element>
      <tag>drawrectangle</tag>
      <style>pushbutton</style>
      <position>260 135 80 20</position>
      <text>Draw Outline</text>
      <callback>ddb_NourishmentsToolbox</callback>
      <option1>drawrectangle</option1>
      <tooltipstring>Draw rectangle to indicate model size</tooltipstring>
    </element> 

  <element>
    <tag>radiocurfile</tag>
    <style>radiobutton</style>
    <position>90 60</position>
    <text>Currents from file</text>
    <variable>handles.Toolbox(tb).Input.currentSource</variable>
    <type>string</type>
    <value>file</value>
    <tooltipstring>Residual currents are read from user-specified file</tooltipstring>
    <dependencies>
      <dependency>
        <action>on</action>
        <tags>
          <tag>radiocuruniform</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.currentSource</variable>
            <value>file</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>radiocuruniform</tag>
    <style>radiobutton</style>
    <position>200 60</position>
    <text>Currents uniform</text>
    <variable>handles.Toolbox(tb).Input.currentSource</variable>
    <type>string</type>
    <value>uniform</value>
    <tooltipstring>Uniform residual currents in U and V direction are defined by user</tooltipstring>
    <dependencies>
      <dependency>
        <action>on</action>
        <tags>
          <tag>radiocurfile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.currentSource</variable>
            <value>uniform</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>


    <element>
      <tag>editcuru</tag>
      <style>edit</style>
      <position>90 40 40 20</position>
      <text>U</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.currentU</variable>
      <type>real</type>
      <tooltipstring>Edit U velocity (m/s)</tooltipstring>

    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>radiocurfile</tag>
          <tag>radiocuruniform</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.currentSource</variable>
            <value>uniform</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>

    </element> 

    <element>
      <tag>editcurv</tag>
      <style>edit</style>
      <position>160 40 40 20</position>
      <text>V</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.currentV</variable>
      <type>real</type>
      <tooltipstring>Edit V velocity (m/s)</tooltipstring>

    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>radiocurfile</tag>
          <tag>radiocuruniform</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.currentSource</variable>
            <value>uniform</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>

    </element> 

  <element>
    <tag>pushselectcurrentsfile</tag>
    <style>pushselectfile</style>
    <position>90 15 110 20</position>
    <text>Currents File</text>
    <variable>handles.Toolbox(tb).Input.currentsFile</variable>
    <type>string</type>
    <extension>*.mat</extension>
    <selectiontext>Select residual currents file</selectiontext>
    <onchange>ddb_NourishmentsToolbox</onchange>
    <option1>loadcurrents</option1>
    <tooltipstring>Select residual currents file</tooltipstring>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>radiocuruniform</tag>
          <tag>radiocurfile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.currentSource</variable>
            <value>file</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>


    <element>
      <tag>drawpolygon</tag>
      <style>pushbutton</style>
      <position>450 130 80 20</position>
      <text>Draw Polygon</text>
      <callback>ddb_NourishmentsToolbox</callback>
      <option1>drawpolygon</option1>
      <tooltipstring>Draw polygon to indicate nourishment location</tooltipstring>
    </element> 


  <element>
    <tag>selecttype</tag>
    <style>popupmenu</style>
    <text>Type</text>
    <position>450 105 80 20</position>
    <type>string</type>
    <variable>handles.Toolbox(tb).Input.nourishmentType</variable>
      <tooltipstring>Select nourishment type (total volume, thickness or height w.r.t. reference level)</tooltipstring>
    <list>
      <texts>
        <text>Volume</text>
        <text>Thickness</text>
        <text>Height</text>
      </texts>
      <values>
        <value>volume</value>
        <value>thickness</value>
        <value>height</value>
      </values>
    </list> 
  </element> 
    <element>
      <tag>editvolume</tag>
      <style>edit</style>
      <position>450 80 80 20</position>
      <text>Volume (m3)</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.nourishmentVolume</variable>
      <type>real</type>
      <tooltipstring>Edit total nourishment volume (volume will be equally distributed within polygon)</tooltipstring>
          <dependencies>
            <dependency>
              <action>enable</action>
              <tags>
                <tag>selecttype</tag>
              </tags>
              <checkfor>all</checkfor>
              <checks>
                <check>
                  <variable>handles.Toolbox(tb).Input.nourishmentType</variable>
                  <value>volume</value>
                  <operator>eq</operator>
                </check>
              </checks>
            </dependency>
          </dependencies>

    </element> 
    <element>
      <tag>editthickness</tag>
      <style>edit</style>
      <position>450 55 80 20</position>
      <text>Thickness (m)</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.nourishmentThickness</variable>
      <type>real</type>
      <tooltipstring>Edit nourishment thickness (thickness will be applied uniformly within polygon)</tooltipstring>
          <dependencies>
            <dependency>
              <action>enable</action>
              <tags>
                <tag>selecttype</tag>
              </tags>
              <checkfor>all</checkfor>
              <checks>
                <check>
                  <variable>handles.Toolbox(tb).Input.nourishmentType</variable>
                  <value>thickness</value>
                  <operator>eq</operator>
                </check>
              </checks>
            </dependency>
          </dependencies>
    </element> 
    <element>
      <tag>editheight</tag>
      <style>edit</style>
      <position>450 30 80 20</position>
      <text>Height (m)</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.nourishmentHeight</variable>
      <type>real</type>
      <tooltipstring>Edit nourishment height w.r.t. reference level (bed level will be raised to this height within polygon)</tooltipstring>
          <dependencies>
            <dependency>
              <action>enable</action>
              <tags>
                <tag>selecttype</tag>
              </tags>
              <checkfor>all</checkfor>
              <checks>
                <check>
                  <variable>handles.Toolbox(tb).Input.nourishmentType</variable>
                  <value>height</value>
                  <operator>eq</operator>
                </check>
              </checks>
            </dependency>
          </dependencies>
    </element> 

    <element>
      <tag>editdiffusion</tag>
      <style>edit</style>
      <position>830 135 40 20</position>
      <text>Diffusion</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.diffusionCoefficient</variable>
      <type>real</type>
      <tooltipstring>Edit diffusion coefficient</tooltipstring>
    </element> 
    <element>
      <tag>editsettlingvelocity</tag>
      <style>edit</style>
      <position>830 110 40 20</position>
      <text>Settling Velocity (m/s)</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.settlingVelocity</variable>
      <type>real</type>
      <tooltipstring>Edit settling velocity</tooltipstring>
    </element> 
    <element>
      <tag>editnryears</tag>
      <style>edit</style>
      <position>830 85 40 20</position>
      <text>Run Time (years)</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.nrYears</variable>
      <type>real</type>
      <tooltipstring>Edit run time (years)</tooltipstring>
    </element> 
    <element>
      <tag>editoutputinterval</tag>
      <style>edit</style>
      <position>830 60 40 20</position>
      <text>Output Interval (years)</text>
      <textposition>left</textposition>
      <variable>handles.Toolbox(tb).Input.outputInterval</variable>
      <type>real</type>
      <tooltipstring>Edit output interval (in years)</tooltipstring>
    </element> 
    <element>
      <tag>computenourishment</tag>
      <style>pushbutton</style>
      <position>830 20 100 20</position>
      <text>Compute</text>
      <callback>ddb_NourishmentsToolbox</callback>
      <option1>computenourishment</option1>
      <tooltipstring>Start computation</tooltipstring>
    </element> 
  </elements> 
</root>
