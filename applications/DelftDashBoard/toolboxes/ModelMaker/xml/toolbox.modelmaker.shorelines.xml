<?xml version="1.0"?>
<root>
  <element>
    <style>pushbutton</style>
    <position>20 110 100 20</position>
    <text>Draw Shoreline</text>
    <callback>ddb_ModelMakerToolbox_shorelines</callback>
    <option1>drawshoreline</option1>
    <tooltipstring>Draw shoreline</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.status</variable>
        <type>string</type>
        <value>waiting</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>pushbutton</style>
    <position>20 85 100 20</position>
    <text>Delete Shoreline</text>
    <callback>ddb_ModelMakerToolbox_shorelines</callback>
    <option1>deleteshoreline</option1>
    <tooltipstring>Delete shoreline</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.shoreline.length</variable>
        <type>integer</type>
        <value>0</value>
        <operator>gt</operator>
      </check>
      <check>
        <variable>handles.model.shorelines.status</variable>
        <type>string</type>
        <value>waiting</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>pushselectfile</style>
    <position>20 60 100 20</position>
    <text>Load Shoreline</text>
    <variable>handles.model.shorelines.domain.shoreline.filename</variable>
    <callback>ddb_ModelMakerToolbox_shorelines</callback>
    <option1>loadshoreline</option1>
    <showfilename>no</showfilename>
    <extension>*.pli</extension>
    <selectiontext>Select shoreline file</selectiontext>
    <tooltipstring>Select shoreline file</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.status</variable>
        <type>string</type>
        <value>waiting</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>pushsavefile</style>
    <position>20 35 100 20</position>
    <text>Save Shoreline</text>
    <variable>handles.model.shorelines.domain.shoreline.filename</variable>
    <callback>ddb_ModelMakerToolbox_shorelines</callback>
    <option1>saveshoreline</option1>
    <showfilename>no</showfilename>
    <extension>*.pli</extension>
    <selectiontext>Select shoreline file</selectiontext>
    <tooltipstring>Save shoreline file</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.shoreline.length</variable>
        <type>integer</type>
        <value>0</value>
        <operator>gt</operator>
      </check>
      <check>
        <variable>handles.model.shorelines.status</variable>
        <type>string</type>
        <value>waiting</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>pushbutton</style>
    <position>870 65 120 20</position>
    <text>START ShorelineS</text>
    <callback>ddb_ModelMakerToolbox_shorelines</callback>
    <option1>startshorelines</option1>
    <tooltipstring>Start ShorelineS model</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.shoreline.length</variable>
        <type>integer</type>
        <value>0</value>
        <operator>gt</operator>
      </check>
      <check>
        <variable>handles.model.shorelines.status</variable>
        <type>string</type>
        <value>waiting</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>pushbutton</style>
    <position>870 40 120 20</position>
    <text>PAUSE/CONTINUE</text>
    <callback>ddb_ModelMakerToolbox_shorelines</callback>
    <option1>pauseshorelines</option1>
    <tooltipstring>Pause ShorelineS model</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.shoreline.length</variable>
        <type>integer</type>
        <value>0</value>
        <operator>gt</operator>
      </check>
      <check>
        <variable>handles.model.shorelines.status</variable>
        <type>string</type>
        <value>waiting</value>
        <operator>ne</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>pushbutton</style>
    <position>870 15 120 20</position>
    <text>STOP</text>
    <callback>ddb_ModelMakerToolbox_shorelines</callback>
    <option1>stopshorelines</option1>
    <tooltipstring>Stop ShorelineS model</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.shoreline.length</variable>
        <type>integer</type>
        <value>0</value>
        <operator>gt</operator>
      </check>
      <check>
        <variable>handles.model.shorelines.status</variable>
        <type>string</type>
        <value>waiting</value>
        <operator>ne</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>text</style>
    <position>870 110 120 20</position>
    <variable>handles.model.shorelines.current_time_string</variable>
    <dependency>
      <action>visible</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.shoreline.length</variable>
        <type>integer</type>
        <value>0</value>
        <operator>gt</operator>
      </check>
      <check>
        <variable>handles.model.shorelines.status</variable>
        <type>string</type>
        <value>waiting</value>
        <operator>ne</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>text</style>
    <position>870 90 120 20</position>
    <variable>handles.model.shorelines.time_remaining_string</variable>
    <dependency>
      <action>visible</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.shoreline.length</variable>
        <type>integer</type>
        <value>0</value>
        <operator>gt</operator>
      </check>
      <check>
        <variable>handles.model.shorelines.status</variable>
        <type>string</type>
        <value>waiting</value>
        <operator>ne</operator>
      </check>
    </dependency>
  </element>
</root>
