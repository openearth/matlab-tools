<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>selectbathymetry</tag>
    <style>listbox</style>
    <text>Available Datasets</text>
    <textposition>above-center</textposition>
    <variable>bathymetry.activeDataset</variable>
    <type>integer</type>
    <position>10 10 180 105</position>
    <tooltipstring>Available bathymetry datasets</tooltipstring>
    <onchange>ddb_ModelMakerToolbox_bathymetry</onchange>
    <option1>selectdataset</option1>
    <list>
      <texts>
        <variable>handles.bathymetry.longNames</variable>
      </texts>
    </list>
  </element>
  <element>
    <tag>usedataset</tag>
    <style>pushbutton</style>
    <position>195 95 80 20</position>
    <text>Use Dataset</text>
    <callback>ddb_ModelMakerToolbox_bathymetry</callback>
    <option1>usedataset</option1>
    <tooltipstring>Use selected dataset (from list on the left)</tooltipstring>
  </element> 
  <element>
    <tag>showdatasetinfo</tag>
    <style>pushbutton</style>
    <position>195 70 80 20</position>
    <text>Info</text>
    <callback>ddb_ModelMakerToolbox_bathymetry</callback>
    <option1>showinfo</option1>
    <tooltipstring>Show meta data of selected dataset</tooltipstring>
  </element> 
  <element>
    <tag>selecteddatasets</tag>
    <style>listbox</style>
    <text>Selection</text>
    <textposition>above-center</textposition>
    <variable>bathymetry.activeSelectedDataset</variable>
    <type>integer</type>
    <position>280 10 150 105</position>
    <tooltipstring>Selected datasets. Datasets should be ordered from high resolution (will be used first) to low resolution.</tooltipstring>
    <onchange>ddb_ModelMakerToolbox_bathymetry</onchange>
    <option1>pickselecteddataset</option1>
    <list>
      <texts>
        <variable>bathymetry.selectedDatasetNames</variable>
      </texts>
    </list>
  </element>
  <element>
    <tag>removedataset</tag>
    <style>pushbutton</style>
    <position>440 95 80 20</position>
    <text>Remove</text>
    <callback>ddb_ModelMakerToolbox_bathymetry</callback>
    <option1>removedataset</option1>
  </element> 
  <element>
    <tag>datasetup</tag>
    <style>pushbutton</style>
    <position>440 70 80 20</position>
    <text>Move Up</text>
    <callback>ddb_ModelMakerToolbox_bathymetry</callback>
    <option1>datasetup</option1>
  </element> 
  <element>
    <tag>datasetdown</tag>
    <style>pushbutton</style>
    <position>440 45 80 20</position>
    <text>Move Down</text>
    <callback>ddb_ModelMakerToolbox_bathymetry</callback>
    <option1>datasetdown</option1>
  </element>  
  <element>
    <tag>editzmax</tag>
    <style>edit</style>
    <position>580 95 50 20</position>
    <text>Z Max</text>
    <textposition>left</textposition>
    <type>real</type>
    <variable>handles.Toolbox(tb).Input.bathymetry.selectedDatasets(handles.Toolbox(tb).Input.bathymetry.activeSelectedDataset).zMax</variable>
    <tooltipstring>Edit maximum height (positive upward) to be used from this dataset.</tooltipstring>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>bathymetry.nrSelectedDatasets</variable>
            <type>integer</type>	              
            <value>0</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element>
  <element>
    <tag>editzmin</tag>
    <style>edit</style>
    <position>580 70 50 20</position>
    <text>Z Min</text>
    <textposition>left</textposition>
    <type>real</type>
    <variable>handles.Toolbox(tb).Input.bathymetry.selectedDatasets(handles.Toolbox(tb).Input.bathymetry.activeSelectedDataset).zMin</variable>
    <tooltipstring>Edit minimum height (positive upward) to be used from this dataset.</tooltipstring>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>bathymetry.nrSelectedDatasets</variable>
            <type>integer</type>	              
            <value>0</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element>
  <element>
    <tag>editdate</tag>
    <style>edit</style>
    <position>760 95 70 20</position>
    <text>Start Date</text>
    <textposition>left</textposition>
    <type>date</type>
    <variable>handles.Toolbox(tb).Input.bathymetry.selectedDatasets(handles.Toolbox(tb).Input.bathymetry.activeSelectedDataset).startDate</variable>
    <tooltipstring>Start date for bathymetry data.</tooltipstring>
    <dependencies>
      <dependency>
        <action>visible</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.bathymetry.selectedDatasets(handles.Toolbox(tb).Input.bathymetry.activeSelectedDataset).type</variable>
            <type>string</type>	              
            <value>kaartblad</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element>
  <element>
    <tag>editdays</tag>
    <style>edit</style>
    <position>760 70 70 20</position>
    <text>Search Interval (days)</text>
    <textposition>left</textposition>
    <type>real</type>
    <variable>handles.Toolbox(tb).Input.bathymetry.selectedDatasets(handles.Toolbox(tb).Input.bathymetry.activeSelectedDataset).searchInterval</variable>
    <tooltipstring>Search interval (relative to start date) for bathymetry data in days. Positive means looking forward in time, negative means looking backward.</tooltipstring>
    <dependencies>
      <dependency>
        <action>visible</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.bathymetry.selectedDatasets(handles.Toolbox(tb).Input.bathymetry.activeSelectedDataset).type</variable>
            <type>string</type>	              
            <value>kaartblad</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element>
  <element>
    <tag>generatebathymetry</tag>
    <style>pushbutton</style>
    <position>910 10 80 20</position>
    <text>Generate</text>
    <callback>ddb_ModelMakerToolbox_bathymetry</callback>
    <option1>generatebathymetry</option1>
    <tooltipstring>Generate model bathymetry</tooltipstring>
  </element> 
</root>
