<?xml version="1.0"?>
<root>
  <element>
    <style>edit</style>
    <position>50 110 50 20</position>
    <tooltipstring>Edit x coordinate or longitude of grid origin</tooltipstring>
    <text>X Origin</text>
    <textposition>left</textposition>
    <onchange>ddb_ModelMakerToolbox_sfincs_quickMode</onchange>
