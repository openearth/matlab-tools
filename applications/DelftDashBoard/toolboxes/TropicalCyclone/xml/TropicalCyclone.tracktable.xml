<root>
  <element>
    <tag>selectquadrant</tag>
    <style>popupmenu</style>
    <position>10 90 80 20</position>
    <text>Quadrant</text>
    <textposition>above-left</textposition>
    <variable>quadrant</variable>
    <type>integer</type>
    <tooltipstring>Select quadrant</tooltipstring>
    <onchange>ddb_TropicalCycloneToolbox_editTrackTable</onchange>
    <option1>selectquadrant</option1>
    <list>
      <texts>
        <text>North East</text>
        <text>North West</text>
        <text>South West</text>
        <text>South East</text>
      </texts>
    </list> 
    <dependencies>
      <dependency>
        <action>visible</action>
        <checkfor>all</checkfor>
        <tags>
          <tag>radiouniform</tag>
          <tag>radioperquadrant</tag>
        </tags> 
        <checks>
          <check>
            <variable>quadrantOption</variable>
            <value>perquadrant</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element> 
  <element>
    <tag>cyclonetable1</tag>
    <style>table</style>
    <position>110 10</position>
    <nrrows>5</nrrows>
    <includenumbers>yes</includenumbers>
    <includebuttons>no</includebuttons>
    <onchange>ddb_TropicalCycloneToolbox_editTrackTable</onchange>
    <option1>edittracktable</option1>
    <columns>
      <column>
        <style>edittime</style>
        <width>110</width>
        <text>Date and time</text>
        <enable>1</enable>
        <variable>trackT</variable>
      </column>
