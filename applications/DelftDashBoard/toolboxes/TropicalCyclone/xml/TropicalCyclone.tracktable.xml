        <style>editreal</style>
        <width>80</width>
        <text>Lat (deg)</text>
        <enable>1</enable>
        <variable>trackY</variable>
      </column>
      <column>
        <style>editreal</style>
        <width>60</width>
        <text>Vmax (kts)</text>
        <enable>1</enable>
        <variable>tableVMax</variable>
      </column>
      <column>
        <style>editreal</style>
        <width>50</width>
        <text>B</text>
        <enable>1</enable>
        <variable>tableB</variable>
      </column>
      <column>
        <style>editreal</style>
        <width>50</width>
        <text>A</text>
        <enable>1</enable>
        <variable>tableA</variable>
      </column>
    </columns>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>nrTrackPoints</variable>
            <type>integer</type>	              
            <value>1</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
      <dependency>
        <action>visible</action>
        <checkfor>all</checkfor>
        <tags>
          <tag>tropicalcyclonepanel.parameters.selectmethod</tag>
        </tags> 
        <checks>
          <check>
            <variable>method</variable>
            <type>integer</type>	              
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>


