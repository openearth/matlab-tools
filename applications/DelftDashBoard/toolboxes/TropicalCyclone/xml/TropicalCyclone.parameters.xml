<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>parameterpanel</tag>
    <style>panel</style>
    <position>10 10 230 130</position>
    <text>Spiderweb Parameters</text>
  </element> 
  <element>
    <tag>editname</tag>
    <style>edit</style>
    <position>80 100 150 20</position>
    <text>Name</text>
    <textposition>left</textposition>
    <variable>handles.Toolbox(tb).Input.name</variable>
    <type>string</type>
    <tooltipstring>Edit name of tropical cyclone</tooltipstring>
  </element> 
  <element>
    <tag>editradius</tag>
    <style>edit</style>
    <position>80 75 50 20</position>
    <text>Radius (km)</text>
    <textposition>left</textposition>
    <variable>handles.Toolbox(tb).Input.radius</variable>
    <type>real</type>
    <tooltipstring>Edit radius of spider web</tooltipstring>
  </element> 
  <element>
    <tag>editdirectionalbins</tag>
    <style>edit</style>
    <position>80 50 50 20</position>
    <text>Dir Bins</text>
    <textposition>left</textposition>
    <variable>handles.Toolbox(tb).Input.nrDirectionalBins</variable>
    <type>integer</type>
    <tooltipstring>Edit number of directional bins in spiderweb file</tooltipstring>
  </element> 
  <element>
    <tag>editradialbins</tag>
    <style>edit</style>
    <position>80 25 50 20</position>
    <text>Radial Bins</text>
    <textposition>left</textposition>
    <variable>handles.Toolbox(tb).Input.nrRadialBins</variable>
    <type>integer</type>
    <tooltipstring>Edit number of radial bins in spiderweb file</tooltipstring>
  </element> 
  <element>
    <tag>methodpanel</tag>
    <style>panel</style>
    <position>250 10 220 130</position>
    <text>Method</text>
  </element> 
  <element>
    <tag>selectmethod</tag>
    <style>popupmenu</style>
    <position>300 100 150 20</position>
    <text>Method</text>
    <textposition>left</textposition>
    <variable>handles.Toolbox(tb).Input.method</variable>
    <type>integer</type>
    <tooltipstring>Select method</tooltipstring>
    <list>
      <texts>
        <text>Holland B & A</text>
        <text>R35, R50, etc.</text>
        <text>Vmax, Pd, Rmax</text>
        <text>Vmax, Pd (Rmax default)</text>
        <text>Vmax, Rmax</text>
        <text>Vmax (Indian data)</text>
      </texts>
    </list> 
  </element> 
  <element>
    <tag>radiouniform</tag>
    <style>radiobutton</style>
    <position>300 75</position>
    <text>Uniform</text>
    <variable>handles.Toolbox(tb).Input.quadrantOption</variable>
    <type>string</type>
    <value>uniform</value>
    <onchange>ddb_TropicalCycloneToolbox_setParameters</onchange>
    <option1>selectquadrantoption</option1>
    <dependencies>
      <dependency>
        <action>on</action>
        <tags>
          <tag>radioperquadrant</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.quadrantOption</variable>
            <value>uniform</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element> 
  <element>
    <tag>radioperquadrant</tag>
    <style>radiobutton</style>
    <position>370 75</position>
    <text>Per Quadrant</text>
    <variable>handles.Toolbox(tb).Input.quadrantOption</variable>
    <type>string</type>
    <value>perquadrant</value>
    <onchange>ddb_TropicalCycloneToolbox_setParameters</onchange>
    <option1>selectquadrantoption</option1>
    <dependencies>
      <dependency>
        <action>on</action>
        <tags>
          <tag>radiouniform</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.quadrantOption</variable>
            <value>perquadrant</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element> 
  <element>
    <tag>drawtrack</tag>
    <style>pushbutton</style>
    <position>480 110 90 20</position>
    <text>Draw Track</text>
    <callback>ddb_TropicalCycloneToolbox_setParameters</callback>
    <option1>drawtrack</option1>
    <tooltipstring>Draw cyclone track</tooltipstring> 
  </element> 
  <element>
    <tag>loaddata</tag>
    <style>pushbutton</style>
    <position>480 85 90 20</position>
    <text>Load Track</text>
    <callback>ddb_TropicalCycloneToolbox_setParameters</callback>
    <option1>loaddata</option1>
    <tooltipstring>Load cyclone data (Delft Dashboard format)</tooltipstring> 
  </element> 
  <element>
    <tag>savedata</tag>
    <style>pushbutton</style>
    <position>480 60 90 20</position>
    <text>Save Track</text>
    <callback>ddb_TropicalCycloneToolbox_setParameters</callback>
    <option1>savedata</option1>
    <tooltipstring>Save cyclone data (Delft Dashboard format)</tooltipstring>
  </element> 
  <element>
    <tag>downloadtrack</tag>
    <style>pushbutton</style>
    <position>480 35 90 20</position>
    <text>Download Track</text>
    <callback>ddb_TropicalCycloneToolbox_setParameters</callback>
    <option1>downloadtrack</option1>
    <tooltipstring>Download track file from external source</tooltipstring>
  </element> 
  <element>
    <tag>selectdownloadlocation</tag>
    <style>popupmenu</style>
    <position>580 35 140 20</position>
    <variable>handles.Toolbox(tb).Input.downloadLocation</variable>
    <type>string</type>
    <tooltipstring>Select download location</tooltipstring>
    <list>
      <texts>
        <variable>handles.Toolbox(tb).Input.downloadLocationNames</variable>
      </texts>
      <values>
        <variable>handles.Toolbox(tb).Input.downloadLocations</variable>
      </values>
    </list> 
  </element> 
  <element>
    <tag>importtrack</tag>
    <style>pushbutton</style>
    <position>480 10 90 20</position>
    <text>Import Track</text>
    <callback>ddb_TropicalCycloneToolbox_setParameters</callback>
    <option1>importtrack</option1>
    <tooltipstring>Import cyclone track file from external source</tooltipstring>
  </element> 
  <element>
    <tag>selectimportformat</tag>
    <style>popupmenu</style>
    <position>580 10 140 20</position>
    <variable>handles.Toolbox(tb).Input.importFormat</variable>
    <type>string</type>
    <tooltipstring>Select import format</tooltipstring>
    <list>
      <texts>
        <variable>handles.Toolbox(tb).Input.importFormatNames</variable>
      </texts>
      <values>
        <variable>handles.Toolbox(tb).Input.importFormats</variable>
      </values>
    </list> 
  </element> 
  <element>
    <tag>computecyclone</tag>
    <style>pushbutton</style>
    <position>750 10 100 20</position>
    <text>Compute Cyclone</text>
    <callback>ddb_TropicalCycloneToolbox_setParameters</callback>
    <option1>computecyclone</option1>
    <tooltipstring>Compute cyclone winds to generate spider web file</tooltipstring> 
  </element> 
  <element>
    <tag>deletetemporaryfiles</tag>
    <style>checkbox</style>
    <position>860 10</position>
    <text>Delete temporary files</text>
    <variable>handles.Toolbox(tb).Input.deleteTemporaryFiles</variable>
    <tooltipstring>Delete temporary files that are made while generating spiderweb file</tooltipstring> 
  </element> 
</root>
