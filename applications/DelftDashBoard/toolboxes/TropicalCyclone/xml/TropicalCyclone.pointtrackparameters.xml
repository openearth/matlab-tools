    <textposition>left</textposition>
    <tooltipstring>Edit maximum wind speed</tooltipstring> 
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.method</variable>
