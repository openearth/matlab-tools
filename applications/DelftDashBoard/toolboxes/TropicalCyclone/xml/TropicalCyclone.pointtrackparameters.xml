<root>
  <title>Cyclone Parameters</title>
  <width>210</width>
  <height>330</height>
  <tag>cycloneparameters</tag>
  <elements>
  <element>
    <tag>edittstart</tag>
    <style>edit</style>
    <enable>0</enable>
    <position>90 295 110 20</position>
    <variable>handles.Toolbox(tb).Input.trackT(handles.Toolbox(tb).Input.activeCyclonePoint)</variable>
    <type>datetime</type>
    <text>Time</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>selectquadrant</tag>
    <style>popupmenu</style>
    <position>90 270 110 20</position>
    <text>Quadrant</text>
    <textposition>left</textposition>
    <enable>1</enable>
    <variable>handles.Toolbox(tb).Input.activeQuadrant</variable>
    <type>integer</type>
    <tooltipstring>Select quadrant</tooltipstring>
    <list>
      <texts>
        <text>North East</text>
        <text>North West</text>
        <text>South West</text>
        <text>South East</text>
      </texts>
    </list> 
    <dependencies>
      <dependency>
        <action>visible</action>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Toolbox(tb).Input.quadrantOption</variable>
            <value>perquadrant</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element> 
