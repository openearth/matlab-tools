          </check>
          <check>
            <variable>handles.Toolbox(tb).Input.method</variable>
            <type>integer</type>	              
            <value>6</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
      <dependency>
        <action>update</action>
        <tags>
          <tag>selectquadrant</tag>
        </tags>
      </dependency>
    </dependencies>
  </element>
