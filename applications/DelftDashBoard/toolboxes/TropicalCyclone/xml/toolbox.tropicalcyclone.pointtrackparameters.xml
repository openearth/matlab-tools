<?xml version="1.0"?>
<root>
  <title>Cyclone Parameters</title>
  <width>210</width>
  <height>330</height>
  <element>
    <style>edit</style>
    <enable>0</enable>
    <position>90 295 110 20</position>
    <variable>handles.toolbox.tropicalcyclone.trackT(handles.toolbox.tropicalcyclone.activeCyclonePoint)</variable>
    <type>datetime</type>
    <text>Time</text>
    <textposition>left</textposition>
  </element>
  <element>
    <style>popupmenu</style>
    <position>90 270 110 20</position>
    <text>Quadrant</text>
    <textposition>left</textposition>
    <enable>1</enable>
    <variable>handles.toolbox.tropicalcyclone.activeQuadrant</variable>
    <type>integer</type>
    <tooltipstring>Select quadrant</tooltipstring>
    <listtext>North East</listtext>
    <listtext>North West</listtext>
    <listtext>South West</listtext>
    <listtext>South East</listtext>
    <dependency>
      <action>visible</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.toolbox.tropicalcyclone.quadrantOption</variable>
        <value>perquadrant</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>edit</style>
    <position>90 245 110 20</position>
    <variable>handles.toolbox.tropicalcyclone.trackVMax(handles.toolbox.tropicalcyclone.activeCyclonePoint,handles.toolbox.tropicalcyclone.activeQuadrant)</variable>
    <type>real</type>
    <text>Vmax (kts)</text>
    <textposition>left</textposition>
    <tooltipstring>Edit maximum wind speed</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>handles.toolbox.tropicalcyclone.method</variable>
        <type>integer</type>
        <value>3</value>
        <operator>eq</operator>
      </check>
      <check>
        <variable>handles.toolbox.tropicalcyclone.method</variable>
        <type>integer</type>
        <value>4</value>
        <operator>eq</operator>
      </check>
      <check>
        <variable>handles.toolbox.tropicalcyclone.method</variable>
        <type>integer</type>
        <value>5</value>
        <operator>eq</operator>
      </check>
      <check>
        <variable>handles.toolbox.tropicalcyclone.method</variable>
        <type>integer</type>
        <value>6</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
