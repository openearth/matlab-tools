<?xml version="1.0"?>
<root>
  <element>
    <tag>selecthisfile</tag>
    <style>pushselectfile</style>
    <position>10 110 120 20</position>
    <text>Select History File</text>
    <variable>trihFile</variable>
    <onchange>ddb_NestingToolbox_nestHD2</onchange>
    <option1>selecthisfile</option1>
    <showfilename>yes</showfilename>
    <extension>trih*.dat</extension>
    <selectiontext>Select Delft3D trih file</selectiontext>
    <tooltipstring>Select Delft3D history (trih) file</tooltipstring>
  </element>
  <element>
    <tag>selectadmfile</tag>
    <style>pushselectfile</style>
    <position>10 85 120 20</position>
    <text>Select Admin File</text>
    <variable>admFile</variable>
    <onchange>ddb_NestingToolbox_nestHD2</onchange>
    <option1>selectadmfile</option1>
    <showfilename>yes</showfilename>
    <extension>*.adm</extension>
    <selectiontext>Select nesting administration file</selectiontext>
    <tooltipstring>Select nesting administration file</tooltipstring>
  </element>
  <element>
    <tag>checkhydro</tag>
    <style>checkbox</style>
    <position>500 85</position>
    <text>Hydrodynamics</text>
    <variable>nestHydro</variable>
    <tooltipstring>Run hydrodynamic nesting. Generates bct file.</tooltipstring>
  </element>
  <element>
    <tag>checktransport</tag>
    <style>checkbox</style>
    <position>500 65</position>
    <text>Transport</text>
    <variable>nestTransport</variable>
    <tooltipstring>Run transport nesting. Generates bcc file.</tooltipstring>
  </element>
  <element>
    <tag>editz0</tag>
    <style>edit</style>
    <position>500 40 80 20</position>
    <text>Water Level Correction (m)</text>
    <variable>zCor</variable>
    <type>real</type>
    <tooltipstring>Edit water level correction. This value will be added to the water levels that are read from the history file.</tooltipstring>
  </element>
  <element>
    <tag>runnesthd2</tag>
    <style>pushbutton</style>
    <position>500 15 80 20</position>
    <text>Run Nesting</text>
    <callback>ddb_NestingToolbox_nestHD2</callback>
    <option1>nesthd2</option1>
    <tooltipstring>Run Nesting</tooltipstring>
  </element>
</root>
