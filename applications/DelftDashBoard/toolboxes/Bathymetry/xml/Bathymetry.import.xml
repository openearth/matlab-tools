<?xml version="1.0"?>
<root>
  <element>
    <style>pushselectfile</style>
    <position>30 110 150 20</position>
    <text>Select Bathy</text>
    <variable>bathyFile</variable>
    <onchange>ddb_BathymetryToolbox_import</onchange>
    <option1>getbathyfile</option1>
    <showfilename>yes</showfilename>
    <filter>
      <extension>*.grd</extension>
      <text>Grid Files (*.grd)</text>
    </filter>
    <filter>
      <extension>*.xyz</extension>
      <text>XYZ files (*.xyz)</text>
    </filter>
    <filter>
      <extension>*.nc</extension>
      <text>NetCDF files (*.nc)</text>
    </filter>
    <selectiontext>Select bathy file</selectiontext>
    <tooltipstring>Select bathymetry file</tooltipstring>
  </element>
  <element>
    <style>pushbutton</style>
    <position>30 80 150 20</position>
    <text>Import Bathy</text>
    <variable>bathyFile</variable>
    <callback>ddb_BathymetryToolbox_import</callback>
    <option1>import</option1>
    <showfilename>no</showfilename>
    <extension>*.nc</extension>
    <selectiontext>Import Data</selectiontext>
    <tooltipstring>Integrate new bathy into GUI</tooltipstring>
  </element>
  <element>
    <style>edit</style>
    <onchange>ddb_BathymetryToolbox_import</onchange>
    <option1>editOutputName</option1>
    <variable>newbathyName</variable>
    <type>string</type>
    <position>195 80 150 20 </position>
  </element>
  <element>
    <style>edit</style>
    <text>Resolution (sec)</text>
    <onchange>ddb_BathymetryToolbox_import</onchange>
    <option1>editresolution</option1>
    <variable>newbathyresolution</variable>
    <type>double</type>
    <position>195 45 75 20 </position>
    <tooltipstring>Set the resolution (in seconds) of the grid.</tooltipstring>
  </element>
  <element>
    <style>popupmenu</style>
    <text>Positive</text>
    <textposition>left</textposition>
    <variable>activeDirection</variable>
    <position>400 45 60 20</position>
    <tooltipstring>Select positive up or down</tooltipstring>
    <type>string</type>
    <listtext>up</listtext>
    <listtext>down</listtext>
    <listvalue>up</listvalue>
    <listvalue>down</listvalue>
  </element>
  <element>
    <style>popupmenu</style>
    <text>Datum Type</text>
    <textposition>left</textposition>
    <variable>datum_type</variable>
    <position>550 80 150 20</position>
    <tooltipstring>Select offset of the Bathy Data</tooltipstring>
    <onchange>ddb_BathymetryToolbox_import</onchange>
    <type>string</type>
    <listtext>Mean Sea Level</listtext>
    <listtext>Low Water Level</listtext>
    <listtext>High Water Level</listtext>
    <listvalue>Mean Sea Level</listvalue>
    <listvalue>Low Water Level</listvalue>
    <listvalue>High Water Level</listvalue>
  </element>
  <element>
    <style>edit</style>
    <text>Offset Value</text>
    <value>0</value>
    <onchange>ddb_BathymetryToolbox_import</onchange>
    <tooltipstring>Set the offset from mean sea level</tooltipstring>
    <option1>offsetvalue</option1>
    <variable>offset_value</variable>
    <type>string</type>
    <position>550 45 50 20 </position>
  </element>
</root>
