<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <toolbox>GeoImage</toolbox>
  <longname>Geo Image</longname>
  <enable>1</enable>
  <elements>
    <element>
      <tag>geoimagepanel</tag>
      <style>panel</style>
      <position>40 10 900 160</position>
      <text>Geo Image</text>
    </element> 
    <element>
      <tag>editxmin</tag>
      <style>edit</style>
      <position>90 130 50 20</position>
      <text>X Min</text>
      <textposition>left</textposition>
      <onchange>ddb_GeoImageToolbox</onchange>
      <option1>editoutline</option1>
      <variable>xLim(1)</variable>
      <type>real</type>
      <tooltipstring>Edit minimum x coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editxmax</tag>
      <style>edit</style>
      <position>90 105 50 20</position>
      <text>X Max</text>
      <textposition>left</textposition>
      <onchange>ddb_GeoImageToolbox</onchange>
      <option1>editoutline</option1>
      <variable>xLim(2)</variable>
      <type>real</type>
      <tooltipstring>Edit maximum x coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editymin</tag>
      <style>edit</style>
      <position>90 80 50 20</position>
      <text>Y Min</text>
      <textposition>left</textposition>
      <onchange>ddb_GeoImageToolbox</onchange>
      <option1>editoutline</option1>
      <variable>yLim(1)</variable>
      <type>real</type>
      <tooltipstring>Edit minimum y coordinate</tooltipstring>
    </element> 
    <element>
      <tag>editymax</tag>
      <style>edit</style>
      <position>90 55 50 20</position>
      <text>Y Max</text>
      <textposition>left</textposition>
      <onchange>ddb_GeoImageToolbox</onchange>
      <option1>editoutline</option1>
      <variable>yLim(2)</variable>
      <type>real</type>
      <tooltipstring>Edit maximum y coordinate</tooltipstring>
    </element> 
    <element>
      <tag>selectzoomlevel</tag>
      <style>popupmenu</style>
      <position>220 130 70 20</position>
      <text>Zoom Level</text>
      <textposition>left</textposition>
      <variable>zoomLevel</variable>
      <type>integer</type>
      <tooltipstring>Select the zoom level. The higher the level, the higher the detail.</tooltipstring>
      <list>
        <texts>
          <variable>zoomLevelStrings</variable>
        </texts>
        <values>
          <value>0</value>
          <value>4</value>
          <value>5</value>
          <value>6</value>
          <value>7</value>
          <value>8</value>
          <value>9</value>
          <value>10</value>
          <value>11</value>
          <value>12</value>
          <value>13</value>
          <value>14</value>
          <value>15</value>
          <value>16</value>
          <value>17</value>
          <value>18</value>
          <value>19</value>
          <value>20</value>
          <value>21</value>
          <value>22</value>
          <value>23</value>
        </values>
      </list>
    </element> 
    <element>
      <tag>editnpix</tag>
      <style>edit</style>
      <position>220 105 70 20</position>
      <text>Nr Pix</text>
      <textposition>left</textposition>
      <variable>nPix</variable>
      <type>integer</type>
      <tooltipstring>Edit number of pixels in horizontal direction.</tooltipstring>
    </element> 
    <element>
      <tag>selecttype</tag>
      <style>popupmenu</style>
      <position>220 80 70 20</position>
      <text>Type</text>
      <textposition>left</textposition>
      <variable>whatKind</variable>
      <type>string</type>
      <list>
        <texts>
          <text>Aerial</text>
          <text>Hybrid</text>
          <text>Road</text>
        </texts>
        <values>
          <value>aerial</value>
          <value>hybrid</value>
          <value>road</value>
        </values>
      </list>
      <tooltipstring>Select image type</tooltipstring>
    </element>         
    <element>
      <tag>drawrectangle</tag>
      <style>pushbutton</style>
      <position>300 130 100 20</position>
      <text>Draw Outline</text>
      <callback>ddb_GeoImageToolbox</callback>
      <option1>drawrectangle</option1>
      <tooltipstring>Draw image rectangle</tooltipstring>
    </element> 
    <element>
      <tag>generateimage</tag>
      <style>pushbutton</style>
      <position>300 105 100 20</position>
      <text>Generate Image</text>
      <callback>ddb_GeoImageToolbox</callback>
      <option1>generateimage</option1>
      <tooltipstring>Download and process image</tooltipstring>
    </element> 
  </elements> 
</root>
