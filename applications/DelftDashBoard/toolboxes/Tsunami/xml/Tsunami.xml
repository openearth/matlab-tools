<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <toolbox>Tsunami</toolbox>
  <longname>Tsunami</longname>
  <enable>1</enable>
  <elements>
    <element>
      <tag>parameterpanel</tag>
      <style>panel</style>
      <position>40 10 300 160</position>
      <text>Earthquake parameters</text>
    </element> 
    <element>
      <tag>editmw</tag>
      <style>edit</style>
      <position>140 130 40 20</position>
      <text>Magnitude (Mw)</text>
      <textposition>left</textposition>
      <onchange>ddb_TsunamiToolbox</onchange>
      <option1>editmw</option1>
      <format>%12.2f</format>
      <variable>handles.Toolbox(tb).Input.Mw</variable>
      <type>real</type>
      <tooltipstring>Edit magnitude (Mw)</tooltipstring>
    </element> 
    <element>
      <tag>edittheoreticallength</tag>
      <style>edit</style>
      <position>140 80 40 20</position>
      <text>Fault Length (km)</text>
      <textposition>left</textposition>
      <onchange>ddb_TsunamiToolbox</onchange>
      <option1>editlength</option1>
      <enable>0</enable>
      <format>%12.1f</format>
      <variable>handles.Toolbox(tb).Input.theoreticalFaultLength</variable>
      <type>real</type>
      <tooltipstring>Edit fault length (km)</tooltipstring>
    </element> 
    <element>
      <tag>editwidth</tag>
      <style>edit</style>
      <position>140 55 40 20</position>
      <text>Fault Width (km)</text>
      <textposition>left</textposition>
      <onchange>ddb_TsunamiToolbox</onchange>
      <option1>editwidth</option1>
      <enable>0</enable>
      <format>%12.1f</format>
      <variable>handles.Toolbox(tb).Input.width</variable>
      <type>real</type>
      <tooltipstring>Edit fault width (km)</tooltipstring>
    </element> 
    <element>
      <tag>editslip</tag>
      <style>edit</style>
      <position>140 30 40 20</position>
      <text>Slip (m)</text>
      <textposition>left</textposition>
      <onchange>ddb_TsunamiToolbox</onchange>
      <option1>editslip</option1>
      <enable>0</enable>
      <format>%12.1f</format>
      <variable>handles.Toolbox(tb).Input.slip</variable>
      <type>real</type>
      <tooltipstring>Edit slip (m)</tooltipstring>
    </element> 
    <element>
      <tag>updatetable</tag>
      <style>checkbox</style>
      <position>190 130</position>
      <text>Auto Update Table</text>
      <variable>handles.Toolbox(tb).Input.updateTable</variable>
      <tooltipstring>Automatically update fault area table when value for Mw or fault line is changed manually</tooltipstring>
    </element> 
    <element>
      <tag>updateparameters</tag>
      <style>checkbox</style>
      <position>190 110</position>
      <text>Auto Update Parameters</text>
      <variable>handles.Toolbox(tb).Input.updateParameters</variable>
      <tooltipstring>Automatically update earthquake parameters when fault line is changed manually</tooltipstring>
    </element> 
    <element>
      <tag>tabelpanel</tag>
      <style>panel</style>
      <position>350 10 650 160</position>
      <text>Fault Area Data</text>
    </element> 
    <element>
      <tag>tsunamitable</tag>
      <style>table</style>
      <position>380 40</position>
      <nrrows>5</nrrows>
      <includenumbers>yes</includenumbers>
      <includebuttons>no</includebuttons>
      <columns>
        <column>
          <style>editreal</style>
          <width>80</width>
          <text>Strike (deg)</text>
          <enable>0</enable>
          <variable>handles.Toolbox(tb).Input.segmentStrike</variable>
        </column>
        <column>
          <style>editreal</style>
          <width>80</width>
          <text>Width (km)</text>
          <enable>1</enable>
          <variable>handles.Toolbox(tb).Input.segmentWidth</variable>
        </column>
        <column>
          <style>editreal</style>
          <width>80</width>
          <text>Depth (km)</text>
          <enable>1</enable>
          <variable>handles.Toolbox(tb).Input.segmentDepth</variable>
        </column>
        <column>
          <style>editreal</style>
          <width>80</width>
          <text>Dip (deg)</text>
          <enable>1</enable>
          <variable>handles.Toolbox(tb).Input.segmentDip</variable>
        </column>
        <column>
          <style>editreal</style>
          <width>80</width>
          <text>Slip/Rake (deg)</text>
          <enable>1</enable>
          <variable>handles.Toolbox(tb).Input.segmentSlipRake</variable>
        </column>
        <column>
          <style>editreal</style>
          <width>80</width>
          <text>Slip (m)</text>
          <enable>1</enable>
          <variable>handles.Toolbox(tb).Input.segmentSlip</variable>
        </column>
      </columns>
    </element>
    <element>
      <tag>editlength</tag>
      <style>edit</style>
      <position>460 15 40 20</position>
      <text>Total Length (km)</text>
      <textposition>left</textposition>
      <onchange>ddb_TsunamiToolbox</onchange>
      <option1>editlength</option1>
      <enable>0</enable>
      <format>%12.1f</format>
      <variable>handles.Toolbox(tb).Input.length</variable>
      <type>real</type>
      <tooltipstring>Edit fault length (km)</tooltipstring>
    </element> 

    <element>
      <tag>loaddata</tag>
      <style>pushbutton</style>
      <position>890 125 100 20</position>
      <text>Load Tsunami</text>
      <callback>ddb_TsunamiToolbox</callback>
      <option1>loaddata</option1>
      <tooltipstring>Load tsunami table data</tooltipstring> 
    </element> 
    <element>
      <tag>savedata</tag>
      <style>pushbutton</style>
      <position>890 100 100 20</position>
      <text>Save Tsunami</text>
      <callback>ddb_TsunamiToolbox</callback>
      <option1>savedata</option1>
      <tooltipstring>Save tsunami table data</tooltipstring>
    </element> 

    <element>
      <tag>drawfaultline</tag>
      <style>pushbutton</style>
      <position>890 45 100 20</position>
      <text>Draw Fault Line</text>
      <callback>ddb_TsunamiToolbox</callback>
      <option1>drawfaultline</option1>
    </element> 
    <element>
      <tag>computewaterlevel</tag>
      <style>pushbutton</style>
      <position>890 20 100 20</position>
      <text>Compute Tsunami</text>
      <callback>ddb_TsunamiToolbox</callback>
      <option1>computewaterlevel</option1>
      <tooltipstring>Compute initial tsunami wave</tooltipstring> 
    </element> 
  </elements>
</root>
