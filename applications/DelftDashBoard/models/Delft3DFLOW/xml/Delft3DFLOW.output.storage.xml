<?xml version="1.0"?>
<root>
  <element>
    <tag>panelmaptimes</tag>
    <style>panel</style>
    <text>Map File</text>
    <position>10 10 210 125</position>
  </element>
  <element>
    <tag>editmapstarttime</tag>
    <style>edit</style>
    <position>100 90 110 20</position>
    <variable>handles.Model(md).Input(ad).mapStartTime</variable>
    <type>datetime</type>
    <tooltipstring>Edit map start time</tooltipstring>
    <text>Start Time</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>editmapstoptime</tag>
    <style>edit</style>
    <position>100 60 110 20</position>
    <variable>handles.Model(md).Input(ad).mapStopTime</variable>
    <type>datetime</type>
    <tooltipstring>Edit map stop time</tooltipstring>
    <text>Stop Time</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>editmaptimestep</tag>
    <style>edit</style>
    <position>100 30 110 20</position>
    <variable>handles.Model(md).Input(ad).mapInterval</variable>
    <type>real</type>
    <tooltipstring>Edit map time step</tooltipstring>
    <text>Time Step (min)</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>panelcomtimes</tag>
    <style>panel</style>
    <text>Communication File</text>
    <position>230 10 210 125</position>
  </element>
  <element>
    <tag>editcomstarttime</tag>
    <style>edit</style>
    <position>320 90 110 20</position>
    <variable>handles.Model(md).Input(ad).comStartTime</variable>
    <type>datetime</type>
    <tooltipstring>Edit com start time</tooltipstring>
    <text>Start Time</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>editcomstoptime</tag>
    <style>edit</style>
    <position>320 60 110 20</position>
    <variable>handles.Model(md).Input(ad).comStopTime</variable>
    <type>datetime</type>
    <tooltipstring>Edit com stop time</tooltipstring>
    <text>Stop Time</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>editcomtimestep</tag>
    <style>edit</style>
    <position>320 30 110 20</position>
    <variable>handles.Model(md).Input(ad).comInterval</variable>
    <type>real</type>
    <tooltipstring>Edit com time step</tooltipstring>
    <text>Time Step (min)</text>
    <textposition>left</textposition>
    <callback>ddb_Delft3DFLOW_storage</callback>
    <option1>editcominterval</option1>
  </element>
  <element>
    <tag>edithistimestep</tag>
    <style>edit</style>
    <position>570 110 50 20</position>
    <variable>handles.Model(md).Input(ad).hisInterval</variable>
    <type>real</type>
    <tooltipstring>Edit history start time</tooltipstring>
    <text>History Time Step (min)</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>editrestartinterval</tag>
    <style>edit</style>
    <position>570 80 50 20</position>
    <variable>handles.Model(md).Input(ad).rstInterval</variable>
    <type>real</type>
    <tooltipstring>Edit restart interval</tooltipstring>
    <text>Restart Interval (min)</text>
    <textposition>left</textposition>
  </element>
  <element>
    <tag>checkonlinevisualization</tag>
    <style>checkbox</style>
    <text>Online Visualization</text>
    <position>460 45</position>
    <variable>handles.Model(md).Input(ad).onlineVisualisation</variable>
  </element>
  <element>
    <tag>checkonlinecoupling</tag>
    <style>checkbox</style>
    <text>Online Coupling</text>
    <position>460 20</position>
    <variable>handles.Model(md).Input(ad).onlineCoupling</variable>
  </element>
  <element>
    <tag>checkairout</tag>
    <style>checkbox</style>
    <text>Atmospheric Forcing</text>
    <position>650 110</position>
    <variable>handles.Model(md).Input(ad).airOut</variable>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.Model(md).Input(ad).wind</variable>
        <value>1</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <tag>checkheatout</tag>
    <style>checkbox</style>
    <text>Heat Exchange</text>
    <position>650 85</position>
    <variable>handles.Model(md).Input(ad).heatOut</variable>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.Model(md).Input(ad).temperature.include</variable>
        <value>1</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
</root>
