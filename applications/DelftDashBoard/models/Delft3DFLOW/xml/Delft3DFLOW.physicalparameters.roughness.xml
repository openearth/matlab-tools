      </texts>
      <values>
        <value>FR84</value>
        <value>MS90</value>
        <value>GM79</value>
        <value>HT91</value>
        <value>DS88</value>
        <value>BK67</value>
        <value>CJ85</value>
        <value>OY88</value>
        <value>VR04</value>
      </values>
    </list> 
  </element> 
