<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>bottomroughnesspanel</tag>
    <style>panel</style>
    <position>10 10 305 128</position>
    <text>Bottom Roughness</text>
  </element>
  <element>
    <tag>selecttype</tag>
    <style>popupmenu</style>
    <text>Roughness Formula</text>
    <position>125 100 130 20</position>
    <variable>handles.Model(md).Input(ad).roughnessType</variable>
    <type>string</type>
    <list>
      <texts>
        <text>Chezy</text>
        <text>Manning</text>
        <text>White-Colebrook</text>
        <text>Z0</text>
      </texts>
      <values>
        <value>C</value>
        <value>M</value>
        <value>W</value>
        <value>Z</value>
      </values>
    </list> 
  </element> 
  <element>
    <tag>radiouniform</tag>
    <style>radiobutton</style>
    <position>20 70</position>
    <text>Uniform</text>
    <variable>handles.Model(md).Input(ad).uniformRoughness</variable>
    <type>integer</type>
    <value>1</value>
    <dependencies>
      <dependency>
        <action>on</action>
        <tags>
          <tag>radiofile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input(ad).uniformRoughness</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>radiofile</tag>
    <style>radiobutton</style>
    <position>20 45</position>
    <text>File</text>
    <variable>handles.Model(md).Input(ad).uniformRoughness</variable>
    <type>integer</type>
    <value>0</value>
    <dependencies>
      <dependency>
        <action>on</action>
        <tags>
          <tag>radiouniform</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input(ad).uniformRoughness</variable>
            <value>0</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>pushselectdepfile</tag>
    <style>pushselectfile</style>
    <position>60 45 50 20</position>
    <text>File</text>
    <variable>handles.Model(md).Input(ad).rghFile</variable>
    <type>string</type>
    <extension>*.rgh</extension>
    <selectiontext>Select roughness file</selectiontext>
    <tooltipstring>Select Delft3D roughness file</tooltipstring>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>radiouniform</tag>
          <tag>radiofile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input(ad).uniformRoughness</variable>
            <value>0</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>editu</tag>
    <style>edit</style>
    <position>125 70 50 20</position>
    <text>U</text>
    <textposition>left</textposition>
    <variable>handles.Model(md).Input(ad).uRoughness</variable>
    <type>real</type>
    <tooltipstring>Enter uniform roughness in U direction</tooltipstring>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>radiouniform</tag>
          <tag>radiofile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input(ad).uniformRoughness</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>editv</tag>
    <style>edit</style>
    <position>195 70 30 20</position>
    <text>V</text>
    <textposition>left</textposition>
    <variable>handles.Model(md).Input(ad).vRoughness</variable>
    <type>real</type>
    <tooltipstring>Enter uniform roughness in V direction</tooltipstring>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
          <tag>radiouniform</tag>
          <tag>radiofile</tag>
        </tags>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input(ad).uniformRoughness</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
  <element>
    <tag>selectwavestresstype</tag>
    <style>popupmenu</style>
    <text>Wave Stress Formula</text>
    <position>125 20 130 20</position>
    <variable>handles.Model(md).Input(ad).rouWav</variable>
    <type>string</type>
    <list>
      <texts>
        <text>Fredsoe</text>
        <text>Myrhaug et al.</text>
        <text>Grant et al.</text>
        <text>Huynh-Thanh et al.</text>
        <text>Davis et al.</text>
        <text>Bijker</text>
        <text>Christoffersen & Jonsson</text>
        <text>O'Connor & Yoo</text>
