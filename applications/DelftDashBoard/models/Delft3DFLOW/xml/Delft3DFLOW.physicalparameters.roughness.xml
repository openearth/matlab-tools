        <value>1</value>
        <value>2</value>
      </values>
    </list> 
  </element> 
  <element>
    <tag>editz0v</tag>
    <style>edit</style>
    <position>450 75 55 20</position>
    <text>Roughness Length (m)</text>
    <textposition>left</textposition>
    <variable>handles.Model(md).Input(ad).z0v</variable>
