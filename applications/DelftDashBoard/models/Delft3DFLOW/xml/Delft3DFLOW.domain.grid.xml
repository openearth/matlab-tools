<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
<element>
  <tag>pushselectgrid</tag>
  <style>pushselectfile</style>
  <position>10 110 130 20</position>
  <tooltipstring>Select grid file</tooltipstring>
  <text>Open grid</text>
  <variable>handles.Model(md).Input(ad).grdFile</variable>
  <extension>*.grd</extension>
  <selectiontext>Select grid File</selectiontext>
  <onchange>ddb_Delft3DFLOW_grid</onchange>
  <option1>selectgrid</option1>
</element>
<element>
  <tag>pushselectenclosure</tag>
  <style>pushselectfile</style>
  <position>10 85 130 20</position>
  <tooltipstring>Select enclosure file</tooltipstring>
  <text>Open grid enclosure</text>
  <variable>handles.Model(md).Input(ad).encFile</variable>
  <extension>*.enc</extension>
  <selectiontext>Select enclosure File</selectiontext>
  <onchange>ddb_Delft3DFLOW_grid</onchange>
  <option1>selectenclosure</option1>
</element>
<element>
  <tag>textgridm</tag>
  <style>text</style>
  <position>10 30</position>
  <prefix>Grid points in M direction : </prefix>
  <type>integer</type>
  <variable>handles.Model(md).Input(ad).MMax</variable>
</element>
<element>
  <tag>textgridn</tag>
  <style>text</style>
  <position>10 10</position>
  <prefix>Grid points in N direction : </prefix>
  <type>integer</type>
  <variable>handles.Model(md).Input(ad).NMax</variable>
</element>
<element>
  <tag>editlatitude</tag>
  <style>edit</style>
  <position>500 35 50 20</position>
  <variable>handles.Model(md).Input(ad).latitude</variable>
  <text>Latitude (deg)</text>
  <type>real</type>
  <dependencies>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <checks>
        <check>
          <variable>handles.screenParameters.coordinateSystem.type</variable>
          <type>string</type>
          <value>cartesian</value>
          <operator>eq</operator>
        </check>
      </checks>
    </dependency>
  </dependencies>
</element>
<element>
  <tag>editorientation</tag>
  <style>edit</style>
  <position>500 10 50 20</position>
  <variable>handles.Model(md).Input(ad).orientation</variable>
  <type>real</type>
  <text>Orientation (deg)</text>
</element>
<element>
  <tag>editkmax</tag>
  <style>edit</style>
  <position>500 60 50 20</position>
  <variable>handles.Model(md).Input(ad).KMax</variable>
  <type>integer</type>
  <text>Number of layers</text>
  <onchange>ddb_Delft3DFLOW_grid</onchange>
  <option1>editkmax</option1>
</element>
<element>
  <tag>pushgeneratelayers</tag>
  <style>pushbutton</style>
  <position>560 60 100 20</position>
  <text>Generate Layers</text>
  <callback>ddb_Delft3DFLOW_grid</callback>
  <option1>generatelayers</option1>
  <dependencies>
    <dependency>
      <tags>
        <tag>editkmax</tag>
      </tags>
      <action>enable</action>
      <checkfor>any</checkfor>
      <checks>
        <check>
          <variable>handles.Model(md).Input(ad).KMax</variable>
          <type>integer</type>
          <value>1</value>
          <operator>gt</operator>
        </check>
      </checks>
    </dependency>
  </dependencies>
</element>
<element>
  <tag>pushsavelayers</tag>
  <style>pushbutton</style>
  <position>560 35 100 20</position>
  <text>Save Layers</text>
  <callback>ddb_Delft3DFLOW_grid</callback>
  <option1>savelayers</option1>
</element>
<element>
  <tag>pushloadlayers</tag>
  <style>pushbutton</style>
  <position>560 10 100 20</position>
  <text>Load Layers</text>
  <callback>ddb_Delft3DFLOW_grid</callback>
  <option1>loadlayers</option1>
</element>
<element>
  <tag>layertable</tag>
  <style>table</style>
  <position>690 25</position>
  <nrrows>5</nrrows>
  <includenumbers>yes</includenumbers>
  <onchange>ddb_Delft3DFLOW_grid</onchange>
  <option1>changelayers</option1>
  <columns>
    <column>
      <style>editreal</style>
      <variable>handles.Model(md).Input(ad).thick</variable>
      <width>50</width>
    </column>
  </columns>
</element>
<element>
  <tag>sumlayers</tag>
  <style>text</style>
  <position>690 5</position>
  <prefix>Sum : </prefix>
  <suffix>%</suffix>
  <type>real</type>
  <variable>handles.Model(md).Input(ad).sumLayers</variable>
</element>
<element>
  <tag>radiosigma</tag>
  <style>radiobutton</style>
  <position>800 85</position>
  <text>Sigma Layers</text>
  <variable>handles.Model(md).Input(ad).layerType</variable>
  <type>string</type>
  <value>Sigma</value>
  <dependencies>
    <dependency>
      <action>on</action>
      <tags>
        <tag>radioz</tag>
      </tags>
      <checkfor>all</checkfor>
      <checks>
        <check>
          <variable>handles.Model(md).Input(ad).layerType</variable>
          <value>Sigma</value>
          <operator>eq</operator>
        </check>
      </checks>
    </dependency>
  </dependencies>
</element>
<element>
  <tag>radioz</tag>
  <style>radiobutton</style>
  <position>800 65</position>
  <text>Z Layers</text>
  <variable>handles.Model(md).Input(ad).layerType</variable>
  <type>string</type>
  <value>Z</value>
  <dependencies>
    <dependency>
      <action>on</action>
      <tags>
        <tag>radiosigma</tag>
      </tags>
      <checkfor>all</checkfor>
      <checks>
        <check>
          <variable>handles.Model(md).Input(ad).layerType</variable>
          <value>Z</value>
          <operator>eq</operator>
        </check>
      </checks>
    </dependency>
  </dependencies>
</element>
<element>
  <tag>editztop</tag>
  <style>edit</style>
  <position>840 35 50 20</position>
  <variable>handles.Model(md).Input(ad).zTop</variable>
  <type>real</type>
  <text>Z top (m)</text>
  <dependencies>
    <dependency>
      <action>enable</action>
      <tags>
        <tag>editkmax</tag>
        <tag>radiosigma</tag>
        <tag>radioz</tag>
      </tags>
      <checkfor>all</checkfor>
      <checks>
        <check>
          <variable>handles.Model(md).Input(ad).layerType</variable>
          <value>Z</value>
          <operator>eq</operator>
        </check>
        <check>
          <variable>handles.Model(md).Input(ad).layerType</variable>
          <value>1</value>
          <operator>gt</operator>
        </check>
      </checks>
    </dependency>
  </dependencies>
</element>
<element>
  <tag>editzbot</tag>
  <style>edit</style>
  <position>840 10 50 20</position>
  <variable>handles.Model(md).Input(ad).zBot</variable>
  <type>real</type>
  <text>Z bottom (m)</text>
  <dependencies>
    <dependency>
      <action>enable</action>
      <tags>
        <tag>editkmax</tag>
        <tag>radiosigma</tag>
        <tag>radioz</tag>
      </tags>
      <checkfor>all</checkfor>
      <checks>
        <check>
          <variable>handles.Model(md).Input(ad).layerType</variable>
          <value>Z</value>
          <operator>eq</operator>
        </check>
        <check>
          <variable>handles.Model(md).Input(ad).layerType</variable>
          <value>1</value>
          <operator>gt</operator>
        </check>
      </checks>
    </dependency>
  </dependencies>
</element>
</root>
