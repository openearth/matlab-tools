<?xml version="1.0"?>
<root>
  <title>Boundary Conditions</title>
  <width>250</width>
  <height>155</height>
  <tag>delft3dwaveboundaryconditions</tag>
  <element>
    <tag>ediths</tag>
    <style>edit</style>
    <position>170 125 50 20</position>
    <variable>handles.Model(md).Input.boundaries(handles.Model(md).Input.activeboundary).waveheight</variable>
    <type>real</type>
    <text>Wave Height Hs (m)</text>
    <textposition>left</textposition>
    <tooltipstring>Edit significant wave height (m)</tooltipstring>
  </element>
  <element>
    <tag>editperiod</tag>
    <style>edit</style>
    <position>170 100 50 20</position>
    <variable>handles.Model(md).Input.boundaries(handles.Model(md).Input.activeboundary).period</variable>
    <type>real</type>
    <text>
      <variable>handles.Model(md).Input.periodtext</variable>
    </text>
    <textposition>left</textposition>
    <tooltipstring>Edit peak wave period (m)</tooltipstring>
  </element>
  <element>
    <tag>editdirection</tag>
    <style>edit</style>
    <position>170 75 50 20</position>
    <variable>handles.Model(md).Input.boundaries(handles.Model(md).Input.activeboundary).direction</variable>
    <type>real</type>
    <text>Wave Direction (deg)</text>
    <textposition>left</textposition>
    <tooltipstring>Edit wave direction (degrees)</tooltipstring>
  </element>
  <element>
    <tag>editdirectionalspreading</tag>
    <style>edit</style>
    <position>170 50 50 20</position>
    <variable>handles.Model(md).Input.boundaries(handles.Model(md).Input.activeboundary).dirspreading</variable>
    <type>real</type>
    <text>
      <variable>handles.Model(md).Input.dirspreadtext</variable>
    </text>
    <textposition>left</textposition>
    <tooltipstring>Edit directional spreading (degrees)</tooltipstring>
  </element>
  <element>
    <tag>pushok</tag>
    <style>pushok</style>
    <position>170 10 50 20</position>
  </element>
  <element>
    <tag>pushcancel</tag>
    <style>pushcancel</style>
    <position>110 10 50 20</position>
  </element>
</root>
