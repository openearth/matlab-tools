<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>editcompgrids</tag>
    <style>listbox</style>
    <position>10 30 200 80</position>
    <tooltipstring>Computational grids</tooltipstring>
    <text>Computational grids :</text>
    <textposition>above-center</textposition>
    <variable>handles.Model(md).Input(ad).ActiveDomain</variable>
    <type>integer</type>
    <list>
      <texts>
        <variable>handles.Model(md).Input(ad).ComputationalGrids</variable>
      </texts>
    </list>
    <onchange>ddb_Delft3DWAVE_nesting</onchange>
    <option1>selectfromlist</option1>
  </element>
  <element>
    <tag>pushaddgrid</tag>
    <style>pushselectfile</style>
    <position>220 90 70 20</position>
    <tooltipstring>Add grid file</tooltipstring>
    <text>Add</text>
    <variable>handles.Model(md).Input(ad).Domain(awg).grdFile</variable>
    <extension>*.grd</extension>
    <selectiontext>Select grid File</selectiontext>
    <onchange>ddb_Delft3DWAVE_nesting</onchange>
    <option1>add</option1>
  </element>
  <element>
    <tag>pushdeletegrid</tag>
    <style>pushbutton</style>
    <position>220 60 70 20</position>
    <tooltipstring>Delete grid file</tooltipstring>
    <text>Delete</text>
    <onchange>ddb_Delft3DWAVE_nesting</onchange>
    <option1>delete</option1>
  </element>
  <element>
    <tag>textcs</tag>
    <style>text</style>
    <position>10 10</position>
    <prefix>Co-ordinate System : </text>
    <type>text</type>
    <variable>handles.Model(md).Input(ad).Domain(awg).Coordsyst</variable>
  </element>  
  <element>
    <tag>textbathygrid</tag>
    <style>text</style>
    <position>360 50</position>
    <prefix>Associated bathymetry grid : </text>
    <type>text</type>
    <variable>handles.Model(md).Input(ad).Domain(awg).GrdFile</variable>
  </element>
  <element>
    <tag>textbathydep</tag>
    <style>text</style>
    <position>360 30</position>
    <prefix>Associated bathymetry data : </text>
    <type>text</type>
    <variable>handles.Model(md).Input(ad).Domain(awg).DepFile</variable>
  </element>
  <element>
    <tag>textnestedin</tag>
    <style>text</style>
    <position>360 10</position>
    <prefix>Nested in : </text>
    <type>text</type>
    <variable>handles.Model(md).Input(ad).Domain(awg).NstFile</variable>
  </element> 
  <element>
    <tag>textmmax</tag>
    <style>text</style>
    <position>680 50</position>
    <prefix>Grid points in M direction : </text>
    <type>text</type>
    <variable>handles.Model(md).Input(ad).Domain(awg).MMax</variable>
  </element>
  <element>
    <tag>textnmax</tag>
    <style>text</style>
    <position>680 30</position>
    <prefix>Grid points in N direction : </text>
    <type>text</type>
    <variable>handles.Model(md).Input(ad).Domain(awg).NMax</variable>
  </element>
</root>