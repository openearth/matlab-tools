<?xml version="1.0"?>
<root>
  <element>
    <tag>panelgrids</tag>
    <style>panel</style>
    <text>Grids</text>
    <position>5 5 890 130</position>
  </element>
  <element>
    <tag>editcompgrids</tag>
    <style>listbox</style>
    <position>10 30 200 80</position>
    <tooltipstring>Computational grids</tooltipstring>
    <text>Computational grids</text>
    <textposition>above-center</textposition>
    <variable>handles.activeWaveGrid</variable>
    <type>integer</type>
    <callback>ddb_Delft3DWAVE_grid</callback>
    <option1>selectgrid</option1>
    <listtext>
      <variable>handles.Model(md).Input.gridnames</variable>
    </listtext>
  </element>
  <element>
    <tag>textcs</tag>
    <style>text</style>
    <position>10 10</position>
    <prefix>Co-ordinate System : </prefix>
    <type>text</type>
    <variable>handles.Model(md).Input.domains(awg).coordsyst</variable>
  </element>
  <element>
    <tag>panelgriddata</tag>
    <style>panel</style>
    <text>Grid data</text>
    <position>440 10 450 120</position>
  </element>
  <element>
    <tag>textgridname</tag>
    <style>text</style>
    <position>450 75</position>
    <prefix>Grid filename : </prefix>
    <type>text</type>
    <variable>handles.Model(md).Input.domains(awg).grid</variable>
  </element>
  <element>
    <tag>textassosgrid</tag>
    <style>text</style>
    <position>450 50</position>
    <prefix>Associated bathymetry grid : </prefix>
    <type>text</type>
    <variable>handles.Model(md).Input.domains(awg).bedlevelgrid</variable>
  </element>
  <element>
    <tag>textassosdep</tag>
    <style>text</style>
    <position>450 25</position>
    <prefix>Associated bathymetry data : </prefix>
    <type>text</type>
    <variable>handles.Model(md).Input.domains(awg).bedlevel</variable>
  </element>
  <element>
    <tag>selectnestedin</tag>
    <style>popupmenu</style>
    <position>510 100 110 15</position>
    <text>Nested in : </text>
    <type>string</type>
    <variable>handles.Model(md).Input.domains(awg).nestgrid</variable>
    <listtext>
      <variable>handles.Model(md).Input.nestgrids</variable>
    </listtext>
    <listvalue>
      <variable>handles.Model(md).Input.nestgrids</variable>
    </listvalue>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.activeWaveGrid</variable>
        <value>1</value>
        <operator>gt</operator>
      </check>
    </dependency>
  </element>
</root>
