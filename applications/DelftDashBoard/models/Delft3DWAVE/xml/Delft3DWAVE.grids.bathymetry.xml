<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>panelgrids</tag>
    <style>panel</style>
    <text>Grids</text>
    <position>5 5 890 130</position>
  </element>
  <element>
    <tag>editcompgrids</tag>
    <style>listbox</style>
    <position>10 30 200 80</position>
    <tooltipstring>Computational grids</tooltipstring>
    <text>Computational Grids</text>
    <textposition>above-center</textposition>
    <variable>handles.activeWaveGrid</variable>
    <type>integer</type>
    <callback>ddb_Delft3DWAVE_grid</callback>
    <option1>selectgrid</option1>
    <list>
      <texts>
        <variable>handles.Model(md).Input.gridnames</variable>
      </texts>
    </list>
  </element>
  <element>
    <tag>textcs</tag>
    <style>text</style>
    <position>10 10</position>
    <prefix>Co-ordinate System : </text>
    <type>text</type>
    <variable>handles.Model(md).Input.domains(awg).coordsyst</variable>
  </element>
  <element>
    <tag>panelbathydata</tag>
    <style>panel</style>
    <text>Bathymetry data</text>
    <position>300 10 590 120</position>  
  </element>
  <element>
    <tag>pushselectbathydata</tag>
    <style>pushselectfile</style>
    <position>305 95 120 20</position>
    <tooltipstring>Select bathymetry file</tooltipstring>
    <text>Select bathymetry data</text>
    <variable>handles.Model(md).Input.domains(awg).bedlevel</variable>
    <extension>*.dep</extension>
    <selectiontext>Select dep File</selectiontext>
    <callback>ddb_Delft3DWAVE_bathymetry</callback>
    <option1>select</option1>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>nrgrids</variable>
            <value>0</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>    
  </element>
</root>
