<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>hydropanel</tag>
    <style>panel</style>
    <position>45 10 900 160</position>
    <title>Hydrodynamics</title>
  </element>
  <element>
    <tag>editcompgrids</tag>
    <style>listbox</style>
    <position>55 20 185 120</position>
    <text>Computational grids</text>
    <textposition>above-center</textposition>
    <variable>handles.activeWaveGrid</variable>
    <type>integer</type>
    <tooltipstring>Select one or more computational grids</tooltipstring>
    <multivariable>activegrids</multivariable>
    <max>1000</max>
    <callback>ddb_Delft3DWAVE_grid</callback>
    <option1>selectgrid</option1>
    <list>
      <texts>
        <variable>handles.Model(md).Input.gridnames</variable>
      </texts>
    </list>
  </element>
  <element>
    <tag>checkhydroflow</tag>
    <style>checkbox</style>
    <position>250 125</position>
    <text>Use hydrodynamics from FLOW</text>
    <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
    <callback>ddb_Delft3DWAVE_hydrodynamics</callback>
    <option1>sethydrodynamics</option1>    
  </element>
  <element>
    <tag>radioddbflow</tag>
    <style>radiobutton</style>
    <position>250 100</position>
    <text>DDB FLOW model</text>
    <type>string</type>
    <variable>handles.Model(md).Input.flowtype</variable>
    <value>ddb</value>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>   
  </element>
  <element>
    <tag>radiootherflow</tag>
    <style>radiobutton</style>
    <position>250 75</position>
    <text>Other FLOW model</text>
    <type>string</type>
    <variable>handles.Model(md).Input.flowtype</variable>
    <value>other</value>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>    
  </element>  
  <element>
    <tag>pushselectmdffile</tag>
    <style>pushselectfile</style>
    <position>270 50 100 20</position>
    <tooltipstring>Select Delft3D-FLOW mdf file</tooltipstring>
    <text>Select FLOW file</text>
    <variable>handles.Model(md).Input.mdffile</variable>
    <extension>*.mdf</extension>
    <selectiontext>Select other MDF File</selectiontext>
    <callback>ddb_Delft3DWAVE_hydrodynamics</callback>
    <option1>sethydrodynamics</option1>        
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.flowtype</variable>
            <value>other</value>
            <operator>eq</operator>
          </check>
          <check>
            <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>          
        </checks>
      </dependency>
    </dependencies>
  </element>  
  <element>
    <tag>textselecthydro</tag>
    <style>text</style>
    <position>500 145</position>
    <text>Select hydrodynamics results from Delft3D-FLOW</text>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>    
  </element>
  <element>
    <tag>checkuseflowbedlevel</tag>
    <style>popupmenu</style>
    <position>500 125 130 20</position>
    <variable>handles.Model(md).Input.domains(awg).flowbedlevel</variable>
    <type>integer</type>
    <tooltipstring>Use bed level from Delft3D-FLOW (overrides bed level from depth file in Delft3D-WAVE)</tooltipstring>
    <text>Bathymetry</text>
    <callback>ddb_Delft3DWAVE_hydrodynamics</callback>
    <option1>checkbedlevel</option1>
    <list>
      <texts>
        <variable>useflowtexts</variable>
      </texts>
      <values>
        <variable>useflowvalues</variable>
      </values>
    </list>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>    
  </element>
  <element>
    <tag>checkuseflowwaterlevel</tag>
    <style>popupmenu</style>
    <position>500 100 130 20</position>
    <variable>handles.Model(md).Input.domains(awg).flowwaterlevel</variable>
    <type>integer</type>
    <tooltipstring>Use water level from Delft3D-FLOW (overrides water level specified in MDW file)</tooltipstring>
    <text>Water level</text>
    <callback>ddb_Delft3DWAVE_hydrodynamics</callback>
    <option1>checkwaterlevel</option1>
    <list>
      <texts>
        <variable>useflowtexts</variable>
      </texts>
      <values>
        <variable>useflowvalues</variable>
      </values>
    </list>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>    
  </element>
  <element>
    <tag>checkuseflowcurrent</tag>
    <style>popupmenu</style>
    <position>500 75 130 20</position>
    <variable>handles.Model(md).Input.domains(awg).flowvelocity</variable>
    <type>integer</type>
    <tooltipstring>Use velocities from Delft3D-FLOW (overrides velocity specified in MDW file)</tooltipstring>
    <text>Current</text>
    <callback>ddb_Delft3DWAVE_hydrodynamics</callback>
    <option1>checkcurrent</option1>
    <list>
      <texts>
        <variable>useflowtexts</variable>
      </texts>
      <values>
        <variable>useflowvalues</variable>
      </values>
    </list>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>    
  </element>
  <element>
    <tag>checkuseflowwind</tag>
    <style>popupmenu</style>
    <position>500 50 130 20</position>
    <variable>handles.Model(md).Input.domains(awg).flowwind</variable>
    <type>integer</type>
    <tooltipstring>Use wind from Delft3D-FLOW (overrides wind specified in MDW file)</tooltipstring>
    <text>Wind</text>
    <callback>ddb_Delft3DWAVE_hydrodynamics</callback>
    <option1>checkwind</option1>
    <list>
      <texts>
        <variable>useflowtexts</variable>
      </texts>
      <values>
        <variable>useflowvalues</variable>
      </values>
    </list>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>    
  </element>
  <element>
    <tag>editwaterlevelcor</tag>
    <style>edit</style>
    <position>780 100 50 20</position>
    <text>Water level correction : </text>
    <textposition>left</textposition>
    <variable>waterlevel</variable>
    <type>real</type>
    <tooltipstring>Enter water level correction</tooltipstring>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>    
  </element>
  <element>
    <tag>textwaterlevelcorunit</tag>
    <style>text</style>
    <position>830 100</position>
    <text>[m]</text>
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.hydrodynamicsflow</variable>
            <value>1</value>
            <operator>eq</operator>
          </check>          
        </checks>
      </dependency>
    </dependencies>    
  </element>  
</root>
