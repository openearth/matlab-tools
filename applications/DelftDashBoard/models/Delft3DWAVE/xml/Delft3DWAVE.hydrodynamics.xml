    <type>text</type>
    <variable>handles.Model(1).Input(1).mdfFile</variable>
    <dependencies>
      <dependency>
        <action>visible</action>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(md).Input.coupling</variable>
            <value>ddbonline</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>    
