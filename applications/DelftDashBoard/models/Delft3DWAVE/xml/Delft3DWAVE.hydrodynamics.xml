    <position>55 125</position>
    <text>Uncoupled</text>
    <type>string</type>
    <variable>handles.Model(md).Input.coupling</variable>
    <value>uncoupled</value>
  </element>  
  <element>
    <tag>radioddbonline</tag>
    <style>radiobutton</style>
    <position>55 100</position>
    <text>Online Dashboard FLOW model</text>
    <type>string</type>
    <variable>handles.Model(md).Input.coupling</variable>
    <value>ddbonline</value>   
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>handles.Model(1).Input(1).MMax</variable>
            <value>0</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>    
  </element>
  <element>
    <tag>textddbflowfile</tag>
    <style>text</style>
    <position>230 100</position>
