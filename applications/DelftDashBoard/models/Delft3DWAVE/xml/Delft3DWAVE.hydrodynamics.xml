<?xml version="1.0"?>
<!-- Written on 25-Mar-2010 19:17:24 using the XML Toolbox for Matlab -->
<root>
  <element>
    <tag>hydropanel</tag>
    <style>panel</style>
    <position>45 10 900 160</position>
    <title>Hydrodynamics</title>
  </element>
  <element>
    <tag>textselecthydro</tag>
    <style>text</style>
    <position>65 145</position>
    <text>Select hydrodynamics results from Delft3D-FLOW</text>
  </element>
  <element>
    <tag>checkuseflowbedlevel</tag>
    <style>checkbox</style>
    <position>65 120</position>
    <variable>
      <name>FlowBedLevel</name>
      <type>boolean</type>
    </variable>
    <tooltipstring>Use bed level from Delft3D-FLOW (overrides bed level from depth file in Delft3D-WAVE)</tooltipstring>
    <text>Bathymetry</text>
  </element>
  <element>
    <tag>checkuseflowwaterlevel</tag>
    <style>checkbox</style>
    <position>65 95</position>
    <variable>
      <name>FlowWaterLevel</name>
      <type>boolean</type>
    </variable>
    <tooltipstring>Use water level from Delft3D-FLOW (overrides water level specified in MDW file)</tooltipstring>
    <text>Water level</text>
  </element>
  <element>
    <tag>checkuseflowcurrent</tag>
    <style>checkbox</style>
    <position>65 70</position>
    <variable>
      <name>FlowVelocity</name>
      <type>boolean</type>
    </variable>
    <tooltipstring>Use velocities from Delft3D-FLOW (overrides velocity specified in MDW file)</tooltipstring>
    <text>Current</text>
  </element>
  <element>
    <tag>checkuseflowwind</tag>
    <style>checkbox</style>
    <position>65 45</position>
    <variable>
      <name>FlowWind</name>
      <type>boolean</type>
    </variable>
    <tooltipstring>Use wind from Delft3D-FLOW (overrides wind specified in MDW file)</tooltipstring>
    <text>Wind</text>
  </element>
  <element>
    <tag>pushselectmdffile</tag>
    <style>pushselectfile</style>
    <position>65 20 100 20</position>
    <tooltipstring>Select Delft3D-FLOW mdf file</tooltipstring>
    <text>Select FLOW file</text>
    <variable>
      <name>MDFFile</name>
      <type>string</type>
    </variable>
    <extension>*.mdw</extension>
    <selectiontext>Select MDF File</selectiontext>
    <onchange>ddb_Delft3DWAVE_selectMDFfile</onchange>
    <dependencies>
      <dependency>
        <action>enable</action>
        <tags>
            <tag>checkuseflowbedlevel</tag>
            <tag>checkuseflowwaterlevel</tag>
            <tag>checkuseflowcurrent</tag>
            <tag>checkuseflowwind</tag>
        </tag>
        <checkfor>any</checkfor>
        <checks>
          <check>
            <variable>
              <name>FlowBedLevel</name>
              <type>boolean</type>
            </variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
          <check>
            <variable>
              <name>FlowWaterLevel</name>
              <type>boolean</type>
            </variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
          <check>
            <variable>
              <name>FlowVelocity</name>
              <type>boolean</type>
            </variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
          <check>
            <variable>
              <name>FlowWind</name>
              <type>boolean</type>
            </variable>
            <value>1</value>
            <operator>eq</operator>
          </check>
        </checks>
      </dependency>
    </dependencies>
  </element>
</root>
