<root>
 <element>
    <tag>radiouniformw</tag>
    <style>radiobutton</style>
    <position>10 110</position>
    <text>Uniform</text>
    <type>string</type>
    <variable>wind</variable>
    <value>uniform</value>
  </element>
  <element>
    <tag>radiospacevaryingw</tag>
