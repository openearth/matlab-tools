<?xml version="1.0"?>
<root>
  <element>
    <tag>activeprocessespanel</tag>
    <style>panel</style>
    <position>5 20 440 120</position>
    <text>Processes activated</text>
  </element>
  <element>
    <tag>checkwind</tag>
    <style>checkbox</style>
    <position>10 80</position>
    <text>Wind Growth</text>
    <type>integer</type>
    <variable>windgrowth</variable>
    <callback>ddb_Delft3DWAVE_physicalParameters</callback>
    <option1>togglewind</option1>
  </element>
  <element>
    <tag>checkquadruplets</tag>
    <style>checkbox</style>
    <position>10 50</position>
    <text>Quadruplets</text>
    <type>integer</type>
    <variable>quadruplets</variable>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>windgrowth</variable>
        <value>1</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <tag>selectwhitecapping</tag>
    <style>popupmenu</style>
    <position>200 80 100 20</position>
    <text>Whitecapping</text>
    <type>string</type>
    <variable>whitecapping</variable>
    <listtext>
      <variable>whitecappingtypestext</variable>
    </listtext>
    <listvalue>
      <variable>whitecappingtypes</variable>
    </listvalue>
    <dependency>
      <action>enable</action>
      <checkfor>all</checkfor>
      <check>
        <variable>windgrowth</variable>
        <value>1</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <tag>checkrefraction</tag>
    <style>checkbox</style>
    <position>600 100</position>
    <text>Refraction</text>
    <type>integer</type>
    <variable>refraction</variable>
  </element>
  <element>
    <tag>checkfreqshiftactivate</tag>
    <style>checkbox</style>
    <position>600 75</position>
    <text>Frequency Shift</text>
    <type>integer</type>
    <variable>freqshift</variable>
  </element>
</root>
