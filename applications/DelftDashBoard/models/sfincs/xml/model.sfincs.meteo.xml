<?xml version="1.0"?>
<root>
  <element>
    <style>panel</style>
    <text>Wind</text>
    <position>40 65 300 105</position>
  </element>
  <element>
    <style>popupmenu</style>
    <position>50 130 130 20</position>
    <text>File Type</text>
    <textposition>right</textposition>
    <variable>handles.model.sfincs.domain(ad).wind_type</variable>
    <type>string</type>
    <listtext>Uniform</listtext>
    <listtext>Rectangular</listtext>
    <listvalue>uniform</listvalue>
    <listvalue>rectangular</listvalue>
  </element>
  <element>
    <style>pushselectfile</style>
    <position>50 100 130 20</position>
    <text>WND File</text>
    <variable>handles.model.sfincs.domain(ad).input.wndfile</variable>
    <showfilename>yes</showfilename>
    <extension>*.wnd</extension>
    <selectiontext>Select wind file</selectiontext>
    <tooltipstring>Select wind file</tooltipstring>
    <dependency>
      <action>visible</action>
      <checkfor>all</checkfor>
      <check>
        <variable>handles.model.sfincs.domain(ad).wind_type</variable>
        <value>uniform</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
