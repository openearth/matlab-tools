<?xml version="1.0"?>
<root>
  <element>
    <style>panel</style>
    <position>40 10 900 160</position>
    <text>Boundary Conditions</text>
  </element>
  <element>
    <style>popupmenu</style>
    <text>Spit overwash</text>
    <position>200 135 60 20</position>
    <type>string</type>
    <variable>handles.model.shorelines.domain.spit_opt</variable>
    <callback>ddb_shorelines_spits_and_channels</callback>
    <option1>selectspitoption</option1>
    <listtext>
      <variable>handles.model.shorelines.domain.spit_opts_long</variable>
    </listtext>
    <listvalue>
      <variable>handles.model.shorelines.domain.spit_opts_short</variable>
    </listvalue>
  </element>
  <element>
    <style>edit</style>
    <position>200 105 60 20</position>
    <text>Spit width</text>
    <variable>handles.model.shorelines.domain.spit_width</variable>
    <type>real</type>
    <tooltipstring>Edit value for spit width</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.spit_opt</variable>
        <value>on</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>edit</style>
    <position>200 80 60 20</position>
    <text>Spit head widtht</text>
    <variable>handles.model.shorelines.domain.spit_headwidth</variable>
    <type>real</type>
    <tooltipstring>Edit spit head widthr</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.spit_opt</variable>
        <value>on</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>edit</style>
    <position>200 55 60 20</position>
    <text>Overwash scale</text>
    <variable>handles.model.shorelines.domain.OWscale</variable>
    <type>real</type>
    <tooltipstring>Edit value for overwash scale</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.spit_opt</variable>
        <value>on</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>edit</style>
    <position>450 105 60 20</position>
    <text>Depth of shoreface Dsf (m)</text>
    <variable>handles.model.shorelines.domain.Dsf</variable>
    <type>real</type>
    <tooltipstring>Edit value for depth of shoreface</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.spit_opt</variable>
        <value>on</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>edit</style>
    <position>450 80 60 20</position>
    <text>Depth of back barrier Dbb (m) (-)</text>
    <variable>handles.model.shorelines.domain.tanbeta</variable>
    <type>real</type>
    <tooltipstring>Edit value for depth of back barrier</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.spit_opt</variable>
        <value>on</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>edit</style>
    <position>450 55 60 20</position>
    <text>Height of berm Bheight (m) (%)</text>
    <variable>handles.model.shorelines.domain.Bheight</variable>
    <type>real</type>
    <tooltipstring>Edit value for percentage of swell</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.spit_opt</variable>
        <value>on</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>popupmenu</style>
    <text>Channel</text>
    <position>650 135 60 20</position>
    <type>string</type>
    <variable>handles.model.shorelines.domain.channel_opt</variable>
    <callback>ddb_shorelines_spits_and_channels</callback>
    <option1>selectchanneloption</option1>
    <listtext>
      <variable>handles.model.shorelines.domain.channel_opts_long</variable>
    </listtext>
    <listvalue>
      <variable>handles.model.shorelines.domain.channel_opts_short</variable>
    </listvalue>
  </element>
  <element>
    <style>edit</style>
    <position>650 105 60 20</position>
    <text>Channel width (m)</text>
    <variable>handles.model.shorelines.domain.channel_width</variable>
    <type>real</type>
    <tooltipstring>Edit value for channel width </tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.channel_opt</variable>
        <value>on</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>edit</style>
    <position>650 80 60 20</position>
    <text>Channel factor</text>
    <variable>handles.model.shorelines.domain.channel_fac</variable>
    <type>real</type>
    <tooltipstring>Edit value for channel factor</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.channel_opt</variable>
        <value>on</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
  <element>
    <style>pushselectfile</style>
    <position>650 55 120 20</position>
    <text>Load River File</text>
    <variable>handles.model.shorelines.domain.LDBriver</variable>
    <callback>ddb_shorelines_spits_and_channels</callback>
    <option1>loadriverfile</option1>
    <showfilename>yes</showfilename>
    <extension>*.txt</extension>
    <selectiontext>Select river file</selectiontext>
    <tooltipstring>Select river file</tooltipstring>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>handles.model.shorelines.domain.channel_opt</variable>
        <value>on</value>
        <operator>eq</operator>
      </check>
    </dependency>
  </element>
</root>
