<?xml version="1.0"?>
<root>
  <model>XBeach</model>
  <longname>XBeach</longname>
  <multipledomains>no</multipledomains>
  <enable>1</enable>
  <element>
    <style>tabpanel</style>
    <tag>XBeach</tag>
    <position>10.000,  10.000,1200.000, 700.000</position>
    <tab>
      <tabstring>Toolbox</tabstring>
      <callback>ddb_selectToolbox</callback>
    </tab>
    <tab>
      <tabstring>Description</tabstring>
      <element>XBeach.description.xml</element>
    </tab>
    <tab>
      <tabstring>Model physics</tabstring>
      <element>XBeach.Physics.xml</element>
    </tab>
    <tab>
      <tabstring>Model domain</tabstring>
      <element>XBeach.Domain.xml</element>
    </tab>
    <tab>
      <tabstring>Time input</tabstring>
      <element>XBeach.Time.xml</element>
    </tab>
    <tab>
      <tabstring>Wave input</tabstring>
      <element>XBeach.Waves.xml</element>
    </tab>
    <tab>
      <tabstring>Flow input</tabstring>
      <element>XBeach.Flow.xml</element>
    </tab>
    <tab>
      <tabstring>Sediment input</tabstring>
      <element>XBeach.Sediment.xml</element>
    </tab>
    <tab>
      <tabstring>Morphology input</tabstring>
      <element>XBeach.Morphology.xml</element>
    </tab>
    <tab>
      <tabstring>Output options</tabstring>
      <element>XBeach.Output.xml</element>
    </tab>
    <tab>
      <tabstring>Advanced input</tabstring>
      <element>XBeach.Advanced.xml</element>
    </tab>
  </element>
  <menu>
    <menuopenfile>
      <menuitem>
        <string>Open Params File</string>
        <callback>ddb_editXBeachDescription</callback>
        <option>open</option>
      </menuitem>
    </menuopenfile>
    <menusavefile>
      <menuitem>
        <string>Save Params File</string>
        <callback>ddb_editXBeachDescription</callback>
        <option>save</option>
      </menuitem>
      <menuitem>
        <string>Save Params File As ...</string>
        <callback>ddb_editXBeachDescription</callback>
        <option>save</option>
      </menuitem>
      <menuitem>
        <string>Save All</string>
        <callback>ddb_editXBeachDescription</callback>
        <option>save</option>
      </menuitem>
      <menuitem>
        <string>Save All As ...</string>
        <callback>ddb_editXBeachDescription</callback>
        <option>save</option>
      </menuitem>
    </menusavefile>
  </menu>
</root>
