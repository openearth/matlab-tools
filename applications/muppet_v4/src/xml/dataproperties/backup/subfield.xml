<?xml version="1.0"?>
<root>
  <element>
    <style>popupmenu</style>
    <type>integer</type>
    <text>Subfield</text>
    <variable>parameters(s.activeparameter).parameter.subfieldnumber</variable>
    <tooltipstring>Select subfield</tooltipstring>
    <callback>muppet_datasetGUI</callback>
    <option1>selectsubfield</option1>
    <position>60 0 270 20</position>
    <list>
      <texts>
        <variable>parameters(s.activeparameter).parameter.subfieldnames</variable>
      </texts>
    </list> 
    <dependencies>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <checks>
          <check>
            <variable>parameters(s.activeparameter).parameter.nrsubfields</variable>
            <value>0</value>
            <operator>gt</operator>
          </check>
        </checks>
      </dependency>
    </dependencies> 
  </element>
</root>
