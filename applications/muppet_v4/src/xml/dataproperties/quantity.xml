<?xml version="1.0"?>
<root>
  <dataproperty>
  <name>quantity</name>
  <element>
    <style>popupmenu</style>
    <text>Quantity</text>
    <variable>quantity</variable>
    <type>string</type>
    <tooltipstring>Select quantity</tooltipstring>
    <position>60 0 270 20</position>
    <callback>muppet_datasetGUI</callback>
    <option1>selectquantity</option1>
    <listtext>
      <variable>quantities</variable>
    </listtext>
    <defaultvalue>scalar</defaultvalue>
    <dependency>
      <action>enable</action>
      <checkfor>any</checkfor>
      <check>
        <variable>nrquantities</variable>
        <value>1</value>
        <operator>gt</operator>
      </check>
    </dependency>
  </element>
  </dataproperty>
</root>
