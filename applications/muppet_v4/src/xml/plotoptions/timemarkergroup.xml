<?xml version="1.0"?>
<root>
  <plotoption>
    <name>timemarkergroup</name>
    <element>
      <name>timemarker</name>
      <keyword>TimeMarker</keyword>
      <default>0</default>
      <style>checkbox</style>
      <type>boolean</type>
      <variable>timemarker</variable>
      <text>Time Marker</text>
      <position>50 0 50 20</position>
    </element>
    <element>
      <keyword>MarkerTime</keyword>
      <variable>markertime</variable>
      <text>Time</text>
      <check>
        <variable>timemarker</variable>
        <operator>eq</operator>
        <value>1</value>
      </check>
      <style>edit</style>
      <position>150 0 120 20</position>
      <type>datetime</type>
      <dependency>
        <action>enable</action>
        <checkfor>all</checkfor>
        <check>
          <variable>timemarker</variable>
          <operator>eq</operator>
          <value>1</value>
        </check>
      </dependency>
    </element>
  </plotoption>
</root>
