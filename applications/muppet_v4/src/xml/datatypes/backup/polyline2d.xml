<?xml version="1.0"?>
<root>
  <name>polyline2d</name>
  <longname>2D Polyline</longname>
  <plotoptionplottype>2d</plotoptionplottype>
  <plot>
    <type>map2d</type>
    <plotroutine>
      <name>plotpolyline</name>
      <plotoption>
        <name>linecolor</name>
        <default>
          <value>black</value>
        </default>
        <default>
          <value>red</value>
        </default>
        <default>
          <value>blue</value>
        </default>
        <default>
          <value>green</value>
        </default>
      </plotoption>
      <plotoption>
        <name>linestyle</name>
        <value>-</value>
      </plotoption>
      <plotoption>
        <name>linewidth</name>
        <value>0.5</value>
      </plotoption>
      <plotoption>
        <name>marker</name>
        <value>none</value>
      </plotoption>
      <plotoption>
        <name>markersize</name>
        <value type='int'>10</value>
      </plotoption>
      <plotoption>
        <name>markeredgecolor</name>
        <value>black</value>
      </plotoption>
      <plotoption>
        <name>markerfacecolor</name>
        <value>black</value>
      </plotoption>
      <plotoption>
        <name>fillclosedpolygons</name>
        <value type='int'>0</value>
      </plotoption>
      <plotoption>
        <name>fillcolor</name>
        <value>lightgreen</value>
      </plotoption>
      <plotoption>
        <name>polygonelevation</name>
        <value>0</value>
      </plotoption>
    </plotroutine>
  </plot>
  <plot>
    <type>xy</type>
    <plotroutine>
      <name>plotpolyline</name>
      <plotoption>
        <name>linecolor</name>
        <default>
          <value>black</value>
        </default>
        <default>
          <value>red</value>
        </default>
        <default>
          <value>blue</value>
        </default>
        <default>
          <value>green</value>
        </default>
      </plotoption>
      <plotoption>
        <name>linestyle</name>
        <value>-</value>
      </plotoption>
      <plotoption>
        <name>linewidth</name>
        <value>0.5</value>
      </plotoption>
      <plotoption>
        <name>marker</name>
        <value>none</value>
      </plotoption>
      <plotoption>
        <name>markersize</name>
        <value type='int'>10</value>
      </plotoption>
      <plotoption>
        <name>markeredgecolor</name>
        <value>black</value>
      </plotoption>
      <plotoption>
        <name>markerfacecolor</name>
        <value>black</value>
      </plotoption>
      <plotoption>
        <name>fillclosedpolygons</name>
        <value type='int'>0</value>
      </plotoption>
      <plotoption>
        <name>fillcolor</name>
        <value>lightgreen</value>
      </plotoption>
    </plotroutine>
  </plot>
</root>
