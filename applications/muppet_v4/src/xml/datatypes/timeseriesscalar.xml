<?xml version="1.0"?>
<root>
  <name>timeseriesscalar</name>
  <longname>Time Series Scalar</longname>
  <plot>
    <type>timeseries</type>
    <plotroutine>
      <name>plotline</name>
      <plotoption>
        <name>plotroutine</name>
      </plotoption>
      <plotoption>
        <name>linegroup</name>
      </plotoption>
      <plotoption>
        <name>markergroup</name>
      </plotoption>
      <plotoption>
        <name>legendtext</name>
      </plotoption>
      <plotoption>
        <name>timebargroup</name>
      </plotoption>
    </plotroutine>
    <plotroutine>
      <name>plotstackedarea</name>
      <plotoption>
        <name>plotroutine</name>
      </plotoption>
      <plotoption>
        <name>fillcolor</name>
        <default>lightgreen</default>
      </plotoption>
      <plotoption>
        <name>linegroup</name>
      </plotoption>
      <plotoption>
        <name>legendtext</name>
      </plotoption>
      <plotoption>
        <name>timebargroup</name>
      </plotoption>
    </plotroutine>
  </plot>
</root>
