<?xml version="1.0" encoding="ISO-8859-1"?>
<settings>
  <basedir>d:\Repositories\oetools_oss\trunk\matlab\applications\ITHK\</basedir>
  <measures>
    <nourishment>
      <THRbeach>0</THRbeach>
      <THRforeshore>500</THRforeshore>
      <THRmega>2500</THRmega>
    </nourishment>
    <beachextension></beachextension>
    <groyne>
      <coastlineupdate>0</coastlineupdate>
      <updatewidth>3</updatewidth>
      <angleshiftclimateA>40</angleshiftclimateA>
      <angleshiftclimateB>15</angleshiftclimateB>
    </groyne>
    <revetment></revetment>
  </measures>
  <indicators>
    <coast>
      <plottype>barplot</plottype>
      <PLOToffset>0</PLOToffset>
    </coast>
    <ecology>
      <benthos>
        <header>Computes population change of benthos at location of nourishment (with classes ranging from Ks*[1, 1-Ythr, 1-2*Ythr, 0]))</header>
        <function>ITHK_ind_ecology_benthos</function>
        <plottype>barplot</plottype>
        <Ythr>0.3</Ythr>
        <PLOTscale1>       1650      </PLOTscale1>
        <PLOTscale2>       0.0       </PLOTscale2>
        <PLOToffset>       7000      </PLOToffset>
        <PLOToffsetDX>     2000      </PLOToffsetDX>
        <icons>
          <class>          1               </class>
          <url> http://127.0.0.1:5000/icons/benthos_similar.png        </url>
          <class>          2               </class>
          <url> http://127.0.0.1:5000/icons/benthos_negative1.png      </url>
          <class>          3               </class>
          <url> http://127.0.0.1:5000/icons/benthos_negative2.png      </url>
          <class>          4               </class>
          <url> http://127.0.0.1:5000/icons/benthos_negative3.png      </url>
        </icons>
      </benthos>
      <juvenilefish>
        <header>Computes relative change of foreshore area (with classes ranging from [1+Ythr : Ythr : 1-2*Ythr]))</header>
        <function>ITHK_ind_ecology_juvenilefish</function>
        <Ythr>0.2</Ythr>
        <ShoreWidth>10000</ShoreWidth>
        <PLOTscale1>       1600      </PLOTscale1>
        <PLOTscale2>       0.0       </PLOTscale2>
        <PLOToffset>       15000     </PLOToffset>
        <icons>
          <class>          1               </class>
          <url> http://127.0.0.1:5000/icons/juvenilefish_positive.png  </url>
          <class>          2               </class>
          <url> http://127.0.0.1:5000/icons/juvenilefish_similar.png   </url>
          <class>          3               </class>
          <url> http://127.0.0.1:5000/icons/juvenilefish_negative1.png </url>
          <class>          4               </class>
          <url> http://127.0.0.1:5000/icons/juvenilefish_negative2.png </url>
        </icons>
      </juvenilefish>
    </ecology>
    <costs>
      <direct>
        <header>Transport distance in kilometers</header>
        <transportdistance>12</transportdistance>
        <header2>Type of location of nourishments 1= predominantly beach; 2= foreshore</header2>
        <locationtype>     1         </locationtype>
        <header3>Ythr = Threshold for costs classes (euro/m along the coast)</header2>
        <Ythr>30000</Ythr>
        <PLOTscale1>       0.000014  </PLOTscale1>
        <PLOTscale2>       0.0       </PLOTscale2>
        <PLOToffset>       17000     </PLOToffset>
        <icons>
          <class>          1               </class>
          <url> http://127.0.0.1:5000/icons/directcosts_no.png         </url>
