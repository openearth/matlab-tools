###############################################################################
#
# simona-scaloost-fijn_exvd-v1
#
###############################################################################
#
# scaloost-fijn model, versie 1
# Kromlijnig Oosterschelde model
#
# ----------------------------------------------------------------------------- 
#
#    Modelnaam       : Scaloost
#
#    Modelversie     : Oorspronkelijke model met aangepaste rand
#                      (verkleinde voordelta)
#
# Gebiedsafbakening  : Voordelta en de Oosterschelde
#
# Literatuur         : Golfberekeningen Oosterschelde
#		       Juli 1998. Rapport A246 Alkyon BV.
# 
# Eigenschappen      : -Kromlijnig rooster
#                      -Mmax = 250   Nmax = 579
#		       -1,1 punt is linksboven
#		       -aandrijving middels H-rand 
#                       over de lokaties : OS11, OS14
#                      -bodemligging uit ?
#                       (recent tov 1998, Rapport A246 Alkyon)
#                      -Vanwege de geringe zoutgradient wordt er niet 
#                       met transport gerekend.
#
# Basisinstellingen  : TSTEP              =  0.25
#		       IDRYflag           =  1
#		       DEPCrit            =  0.10
#		       EDDYviscositycoeff =  1.0
#
# Specifieke 
#          kenmerken : -Rooster is genest in Kuststrook (1998) met een 
#                       verdichtingsfactor 3.
#                      -Fijn grid, zodat elke schuif van de SVKO als
#                       barrier is gemodelleerd.
#
# ------------------------------------------------------------------------------ 
#                      
# Mutaties
# Datum     Naam       Omschrijving
#  1 aug 2006  EC      In DRYING:
#                         Verwijderd: IDRYflag = 1
#                         Toegevoegd: CHECK_WL    ='YES'
#                                     UPWIND_ZETA ='NO'
#  1 aug 2007  EC      In FLOW, PROBLEM, METHODVARIABles
#                         Gewijzigd:  ITERMOm = 8 (was 2)
#  1 aug 2008  EC      In FLOW, PROBLEM, METHODVARIABles
#                         Toegevoegd:  CHECKCont    =  'vel'
#                                      ITERACCURVel = 0.0005
#                         Verwijderd:  ITERACCuracy = 0.0005
#
#
# ------------------------------------------------------------------------------ 

IDENTification
  WAQUA
  EXPERIMENT =  'oost' OVERWRITE
  MODID      =  'Scaloost'
  TITLE      =  'Scaloost-fijn_exvd'


# ------------------------------------------------------------------------------ 
MESH
  GRID
    AREA
      MMAX      =   250
      NMAX      =   579
      ANGLEgrid =   .00 
      LATItude  = 52.00
    CURVilinear RGFfile      = '../rooster/rgf'
  POINTS        INCLUDE FILE = '../locaties/points'
                INCLUDE FILE = '../kunstwerken/bar-points'
# CURVEs        INCLUDE FILE = '../locaties/curves'
  BOUNdaries
    ENCLosures  INCLUDE FILE = '../randen/comgrid'
    OPENings    INCLUDE FILE = '../randen/openings'
    BARriers    INCLUDE FILE = '../kunstwerken/bar-def'
  BATHYMetry
    GLOBAL
      DPD_GIVEN
      METH_DPS      = 'MEAN_DPD'
      DEPMULTiplier =  1.00       
      THREShold     =   .30
      LAYOUT        =     1
      DEPDEF        =  6.00
    LOCAL       INCLUDE file = '../bodem/bathymetry'
  DRYPoints
    DAMPOINTS   INCLUDE file = '../schotjes/dampoints'
    CLOSEU      INCLUDE file = '../schotjes/closeu'
    CLOSEV      INCLUDE file = '../schotjes/closev'


# ------------------------------------------------------------------------------ 
GENERAL
# DIFFusion
  PHYSical_parameters
    GRAVity           =    9.8130
    WATDENsity        =    1023.0
    AIRDENsity        =    1.2050
  WIND
    WCONVersionfactor =   1.00000
    WUNIT             =     'm/s'
    VARIABLE_Cd:
      CdA             =   0.00144   
      CdB             =   0.00258
      WIND_CdA        =     10.20
      WIND_CdB        =     15.90
      INCLUDE FILE    = 'timeser-wind'


# ------------------------------------------------------------------------------ 
FLOW
  PROBlem
    TIMEFRame
      DATE         = '23 JUL 1994'
      TSTART       =      0.0
      TSTOP        =   4310.0
      TIMEZONE     =    'MET'
    METHODvariables
      TSTEP        =     0.25
      ITERCOn      =       20
      ITERMOm      =        8
      CHECKCont    =    'vel'
      ITERACCURVel =   0.0005
    SMOOTHing
      TLSMOOTH     =    180.0
    DRYING  
      CHECK_WL     =    'YES'
      DEPCrit      =     0.10
      UPWIND_ZETA  =     'NO'
    FRICTion
      GLObal     TICVal       =    30.00
                 FORMula      = 'Manning'
      UDIRec
        GLOBAL   CONST_values =   .0240
#       LOCAL    INCLUDE FILE = '../ruwheid/frictionatu'
      VDIRec
        GLOBAL   CONST_values =   .0240
#       LOCAL    INCLUDE FILE = '../ruwheid/frictionatv'
     VISCOsity 
        EDDYviscositycoeff =  1.000
     BARRIERcoefficients    INCLUDE FILE = '../kunstwerken/bar-coef'
  FORCings
    INITial 
      WATLEVel      GLOBAL  CONST_values = 1.500
#                   LOCAL   INCLUDE FILE = '../initieel/wlevel'
    BOUNDAries      INCLUDE FILE = '../randen/bound-flow'
    TIMESERIes      INCLUDE FILE = 'timeser-rvw'
    BARRiers        INCLUDE FILE = '../kunstwerken/bar-flow'
  CHECKPoints
    LEVELStations   INCLUDE FILE = '../uitvoer/check-wl'
    CURRENtstations INCLUDE FILE = '../uitvoer/check-cur'
#   USECTions       INCLUDE FILE = '../uitvoer/sectu'
#   VSECTions       INCLUDE FILE = '../uitvoer/sectv'


# ------------------------------------------------------------------------------ 
SDSOUTput
  MAPS
    TFMAPs    =  1440.0
    TIMAPs    =    60.0
    TLMAPs    =  4260.0

  HISTories
    TFHISto   =     0.0
    TIHISto   =    10.0
    TLHISto   =  4310.0

# RESTART
#   TFREStart =  7200.0
#   TIREStart =  1440.0
#   TLREStart =  8640.0


# ------------------------------------------------------------------------------ 
PRINToutput
  CONTROL
    TFRAMEStat=(   20.0,    30.000,  4310.0)
# ------------------------------------------------------------------------------ 
